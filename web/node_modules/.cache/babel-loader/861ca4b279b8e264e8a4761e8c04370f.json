A,GAxBA;AAyBA,EAAA,OAzBA,qBAyBA,CAAA,CAzBA;AA0BA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,CAAA,KAAA,QAAA,IAAA,KAAA,QAAA,CAAA,MAAA,IAAA,CAAA,EAAA,OAAA,KAAA;AAEA,aAAA,IAAA;AACA,KALA;AAMA,IAAA,UANA,wBAMA;AACA,WAAA,KAAA,GAAA,IAAA;AACA,KARA;AASA,IAAA,OATA,mBASA,EATA,EASA;AAAA;;AACA,WAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,KAAA,UAAA,GAAA,EAAA,GAAA,GAAA,GAAA,KAAA,SADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,YAAA,MAAA,GAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,EAAA,CAAA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EAAA,MAAA;;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,MAAA,CAAA,MAAA,EAAA,CAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,CAAA,uBAAA,CADA;AAEA,UAAA,IAAA,EAAA,SAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAbA,WAcA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,IAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OArBA;AAsBA;AAhCA;AA1BA,CAAA","sourcesContent":["<template>\r\n  <b-button\r\n    :disabled=\"TemArquivo() ? null : 'disabled'\"\r\n    class=\"btn btn-secondary mr-2\"\r\n    @click=\"AbrirModal()\"\r\n    :title=\"TemArquivo() ? 'Visualizar arquivos' : 'Nenhum arquivo'\"\r\n  >\r\n    <i class=\"fas fa-download text-black\"></i>\r\n    <b-modal\r\n      v-model=\"abrir\"\r\n      title=\"Arquivo(s)\"\r\n      size=\"sm\"\r\n      scrollable\r\n      hide-footer\r\n    >\r\n      <div class=\"row\" v-for=\"(item, index) in arquivos\" :key=\"index\">\r\n        <div class=\"col-5\">\r\n          <a\r\n            class=\"btn btn-primary mb-1\"\r\n            :href=\"$store.getters.baseURL + urlDownload + item\"\r\n            target=\"_blank\"\r\n            >Arquivo {{ index + 1 }}</a\r\n          >\r\n        </div>\r\n        <div class=\"col-3\">\r\n          <button class=\"btn btn-danger mb-1\" @click=\"Remover(item)\">\r\n            Remover\r\n          </button>\r\n        </div>\r\n      </div>\r\n      <div class=\"row\" v-if=\"!TemArquivo()\">\r\n        <div class=\"col-6\">\r\n          <span>Nenhum arquivo...</span>\r\n        </div>\r\n      </div>\r\n    </b-modal>\r\n  </b-button>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  components: {},\r\n  props: {\r\n    arquivos: {\r\n      type: Array,\r\n      default: []\r\n    },\r\n    urlDownload: {\r\n      type: String,\r\n      default: \"\"\r\n    },\r\n    urlRemover: {\r\n      type: String,\r\n      default: \"\"\r\n    },\r\n    vinculoId: {\r\n      type: String,\r\n      default: \"\"\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      abrir: false\r\n    };\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    TemArquivo() {\r\n      if (!this.arquivos || this.arquivos.length <= 0) return false;\r\n\r\n      return true;\r\n    },\r\n    AbrirModal() {\r\n      this.abrir = true;\r\n    },\r\n    Remover(id) {\r\n      this.$http({\r\n        url: this.urlRemover + id + \"/\" + this.vinculoId,\r\n        method: \"DELETE\"\r\n      })\r\n        .then(() => {\r\n          let indice = this.arquivos.indexOf(id);\r\n          console.log(\"indice\", indice);\r\n          this.arquivos.splice(indice, 1);\r\n          this.$notify({\r\n            data: [\"Removido com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          console.log(\"erro\", erro);\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}