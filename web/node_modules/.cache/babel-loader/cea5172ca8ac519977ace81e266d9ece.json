,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,cAAA;AACA,OAvBA,WAwBA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OA/BA;AAgCA,KA3CA;AA4CA,IAAA,MA5CA,oBA4CA;AACA,aAAA,KAAA,QAAA,KAAA,KAAA,MAAA,CAAA,OAAA,CAAA,SAAA;AACA,KA9CA;AA+CA,IAAA,WA/CA,uBA+CA,MA/CA,EA+CA;AACA,MAAA,MAAA,CAAA,cAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,kBAAA,GAAA,CAAA;AACA,WAAA,mBAAA,GAAA,CAAA;AACA,KApDA;AAqDA,IAAA,OArDA,mBAqDA,MArDA,EAqDA;AAAA;;AACA,MAAA,MAAA,CAAA,cAAA;AACA,WAAA,YAAA,GAAA,KAAA;AAEA,UAAA,CAAA,KAAA,WAAA,EAAA;AAEA,MAAA,yBAAA,CAAA,OAAA,CAAA,KAAA,WAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,CAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,CAAA,gCAAA,CADA;AAEA,UAAA,IAAA,EAAA,SAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OARA,WASA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAfA;AAgBA,KA3EA;AA4EA,IAAA,OA5EA,mBA4EA,IA5EA,EA4EA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,WAAA,WAAA,GAAA,IAAA,CAAA,EAAA;AACA,KA/EA;AAgFA,IAAA,YAhFA,wBAgFA,KAhFA,EAgFA;AACA,aAAA,IAAA,IAAA,CAAA,KAAA,EAAA,kBAAA,EAAA;AACA,KAlFA;AAmFA,IAAA,YAnFA,wBAmFA,KAnFA,EAmFA;AACA,UAAA,KAAA,EAAA;AACA,eAAA,KAAA,CAAA,cAAA,CAAA,OAAA,EAAA;AACA,UAAA,KAAA,EAAA,UADA;AAEA,UAAA,QAAA,EAAA;AAFA,SAAA,CAAA;AAIA,OALA,MAKA;AACA,eAAA,GAAA,CAAA,cAAA,CAAA,OAAA,EAAA;AACA,UAAA,KAAA,EAAA,UADA;AAEA,UAAA,QAAA,EAAA;AAFA,SAAA,CAAA;AAIA;AACA,KA/FA;AAgGA,IAAA,iBAhGA,6BAgGA,KAhGA,EAgGA;AACA,aAAA,KAAA,GAAA,KAAA,GAAA,CAAA;AACA;AAlGA;AAhDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"animated fadeIn\">\r\n    <div v-if=\"loading\" class=\"loading-container\">\r\n      <RotateSquare\r\n        class=\"loading-position animated fadeIn\"\r\n        size=\"60px\"\r\n      ></RotateSquare>\r\n    </div>\r\n    <div v-else>\r\n      <div class=\"row\">\r\n        <div class=\"col-sm-12 col-md-12 col-lg-12 col-xl-12\">\r\n          <div class=\"card\">\r\n            <header class=\"card-header\" @click=\"abrir = !abrir\">\r\n              <div class=\"d-flex\">\r\n                <strong class=\"align-self-center\">Contrato(s)</strong>\r\n                <small class=\"ml-2 mt-1\">Clique para abrir/esconder</small>\r\n\r\n                <i\r\n                  :class=\"\r\n                    abrir\r\n                      ? 'ml-auto mt-1 fas fa-chevron-up'\r\n                      : 'ml-auto mt-1 fas fa-chevron-down'\r\n                  \"\r\n                ></i>\r\n              </div>\r\n            </header>\r\n            <div :class=\"abrir ? 'collapse-show' : 'collapse'\">\r\n              <div class=\"card-body\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-lg-5 col-md-6 col-sm-12\">\r\n                    <div class=\"form-group\">\r\n                      <label>Número</label>\r\n                      <input\r\n                        type=\"text\"\r\n                        v-model=\"filtro.numero\"\r\n                        class=\"form-control\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-lg-4 col-md-5 col-sm-12 mt-4\">\r\n                    <button\r\n                      class=\"btn btn-primary mr-2\"\r\n                      type=\"button\"\r\n                      @click=\"ObterGrid(1)\"\r\n                    >\r\n                      Filtrar\r\n                    </button>\r\n                    <button\r\n                      class=\"btn btn-secondary\"\r\n                      type=\"button\"\r\n                      @click=\"Limpar()\"\r\n                    >\r\n                      Limpar\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n\r\n                <b-table\r\n                  :hover=\"true\"\r\n                  responsive\r\n                  :items=\"itens\"\r\n                  :fields=\"fields\"\r\n                  striped\r\n                  :per-page=\"itensPorPagina\"\r\n                  show-empty\r\n                  empty-text=\"Nenhum contrato encontrado.\"\r\n                >\r\n                  <template v-slot:empty=\"scope\">\r\n                    <h4>{{ scope.emptyText }}</h4>\r\n                  </template>\r\n                  <template v-slot:cell(acoes)=\"data\">\r\n                    <div class=\"btn-group-sm\">\r\n                      <b-button\r\n                        variant=\"warning\"\r\n                        style=\"margin-right: 10px\"\r\n                        title=\"Editar\"\r\n                        @click=\"Editar(data.item)\"\r\n                      >\r\n                        <i class=\"fa fa-edit\"></i>\r\n                      </b-button>\r\n                      <b-button\r\n                        variant=\"danger\"\r\n                        title=\"Remover\"\r\n                        @click=\"Remover(data.item)\"\r\n                      >\r\n                        <i class=\"fas fa-trash-alt\"></i>\r\n                      </b-button>\r\n                    </div>\r\n                  </template>\r\n                  <template v-slot:cell(dataInicio)=\"data\">\r\n                    <div class=\"center\">\r\n                      <span>{{ data.item.dataInicio }}</span>\r\n                    </div>\r\n                  </template>\r\n                  <template v-slot:cell(dataTermino)=\"data\">\r\n                    <div class=\"center\">\r\n                      <span>{{ data.item.dataTermino }}</span>\r\n                    </div>\r\n                  </template>\r\n                  <template v-slot:cell(valorLimite)=\"data\">\r\n                    <div class=\"left\">\r\n                      <span>{{ FormataValor(data.item.valorLimite) }}</span>\r\n                    </div>\r\n                  </template>\r\n                  <template v-slot:cell(valor)=\"data\">\r\n                    <div class=\"left\">\r\n                      <span>{{ FormataValor(data.item.valor) }}</span>\r\n                    </div>\r\n                  </template>\r\n                  <template v-slot:cell(quantidadeLimite)=\"data\">\r\n                    <div class=\"left\">\r\n                      <span>{{\r\n                        FormataQuantidade(data.item.quantidadeLimite)\r\n                      }}</span>\r\n                    </div>\r\n                  </template>\r\n                </b-table>\r\n                <b-pagination\r\n                  v-model=\"pagina\"\r\n                  :total-rows=\"total\"\r\n                  :per-page=\"itensPorPagina\"\r\n                  align=\"right\"\r\n                  size=\"md\"\r\n                  class=\"mt-2\"\r\n                ></b-pagination>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <b-modal\r\n      v-model=\"modalRemover\"\r\n      title=\"Confirmar exclusão\"\r\n      class=\"modal-danger\"\r\n      ok-variant=\"danger\"\r\n      @ok=\"ModalOk\"\r\n      @hidden=\"ModalCancel\"\r\n    >\r\n      Você confirma a exclusão desse registro?\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport ContratoFornecedorServico from \"../../servico/ContratoFornecedorServico\";\r\nimport TipoPessoaContratoEnum from \"../../enums/TipoPessoaContratoEnum\";\r\nimport DateTime from \"../../util/DateTime\";\r\n\r\nexport default {\r\n  name: \"FornecedorContrato\",\r\n  components: {\r\n    RotateSquare,\r\n    DateTime\r\n  },\r\n  props: {\r\n    pessoaId: {\r\n      type: String,\r\n      default: \"\"\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      modalRemover: false,\r\n      itemRemover: null,\r\n      abrir: false,\r\n      itens: [],\r\n      pagina: 1,\r\n      total: 0,\r\n      itensPorPagina: 20,\r\n      filtro: { numero: \"\" },\r\n      fields: [\r\n        { key: \"numero\", label: \"Número\", sortable: true },\r\n        { key: \"entidadeLicitacao\", label: \"Entidade\", sortable: true },\r\n        { key: \"valorLimite\", label: \"Valor Limite\", sortable: true },\r\n        { key: \"quantidadeLimite\", label: \"Quantidade Limite\", sortable: true },\r\n        { key: \"dataInicio\", label: \"Data Início\", sortable: true },\r\n        { key: \"dataTermino\", label: \"Data Término\", sortable: true },\r\n        { key: \"valor\", label: \"Valor Contrato\", sortable: true },\r\n        {\r\n          key: \"acoes\",\r\n          label: \"Ações\",\r\n          sortable: false,\r\n          thClass: \"center, wd-120-px\"\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  watch: {\r\n    pagina: function (val) {\r\n      this.ObterGrid(val);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.ObterGrid(1);\r\n  },\r\n  methods: {\r\n    Limpar() {\r\n      this.filtro.numero = \"\";\r\n      this.ObterGrid(1);\r\n    },\r\n    Editar(contrato) {\r\n      this.$router.push(\"/contrato/editar/\" + contrato.contratoId);\r\n    },\r\n    ObterGrid(pagina) {\r\n      this.loading = false;\r\n\r\n      this.$http({\r\n        url:\r\n          \"/pessoacontrato/obter-grid?pagina=\" +\r\n          pagina +\r\n          \"&numero=\" +\r\n          this.filtro.numero +\r\n          \"&TipoPessoaContrato=\" +\r\n          TipoPessoaContratoEnum.Fornecedor +\r\n          \"&pessoaId=\" +\r\n          this.pessoaId,\r\n        method: \"GET\"\r\n      })\r\n        .then((response) => {\r\n          this.loading = false;\r\n          response.data.itens.forEach((item) => {\r\n            item.dataInicio = new Date(item.dataInicio).toLocaleDateString();\r\n            item.dataTermino = new Date(item.dataTermino).toLocaleDateString();\r\n          });\r\n\r\n          this.itens = response.data.itens;\r\n          this.total = response.data.total;\r\n          this.pagina = response.data.pagina;\r\n          this.itensPorPagina = response.data.itensPorPagina;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    IsNovo() {\r\n      return this.pessoaId === this.$store.getters.emptyGuid;\r\n    },\r\n    ModalCancel(evento) {\r\n      evento.preventDefault();\r\n      this.itemRemover = null;\r\n      this.addTodosQuantidade = 0;\r\n      this.addTodosValorLimite = 0;\r\n    },\r\n    ModalOk(evento) {\r\n      evento.preventDefault();\r\n      this.modalRemover = false;\r\n\r\n      if (!this.itemRemover) return;\r\n\r\n      ContratoFornecedorServico.Remover(this.itemRemover)\r\n        .then(() => {\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Contrato removido com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    Remover(item) {\r\n      this.modalRemover = true;\r\n      this.itemRemover = item.id;\r\n    },\r\n    formatarData(value) {\r\n      return new Date(value).toLocaleDateString();\r\n    },\r\n    FormataValor(valor) {\r\n      if (valor) {\r\n        return valor.toLocaleString(\"pt-br\", {\r\n          style: \"currency\",\r\n          currency: \"BRL\"\r\n        });\r\n      } else {\r\n        return (0.0).toLocaleString(\"pt-br\", {\r\n          style: \"currency\",\r\n          currency: \"BRL\"\r\n        });\r\n      }\r\n    },\r\n    FormataQuantidade(valor) {\r\n      return valor ? valor : 0;\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Fornecedor"}]}