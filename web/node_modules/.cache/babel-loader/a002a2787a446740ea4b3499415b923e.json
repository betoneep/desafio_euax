,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAbA;AAcA;AA3FA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"animated fadeIn\">\r\n    <div v-if=\"loading\" class=\"loading-container\">\r\n      <RotateSquare\r\n        class=\"loading-position animated fadeIn\"\r\n        size=\"60px\"\r\n      ></RotateSquare>\r\n    </div>\r\n    <form v-else @submit=\"ValidarForm\">\r\n      <div class=\"row\">\r\n        <div class=\"col-sm-12 col-md-12 col-lg-12 col-xl-12\">\r\n          <div class=\"card\">\r\n            <header class=\"card-header\">\r\n              <strong class=\"align-self-center\"\r\n                >{{\r\n                  viewModel.id == store.state.emptyGuid\r\n                    ? 'Novo Produto'\r\n                    : 'Editar Produto'\r\n                }}\r\n              </strong>\r\n            </header>\r\n            <div class=\"card-body\">\r\n              <div class=\"row\">\r\n                <div class=\"col\">\r\n                  <div class=\"form-group\">\r\n                    <small>Campos com * são de preenchimento obrigatório</small>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"col-sm-12 col-md-4 col-lg-4 col-xl-4\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Descrição</label>\r\n                    <input\r\n                      v-model=\"viewModel.descricao\"\r\n                      class=\"form-control\"\r\n                      type=\"text\"\r\n                      placeholder=\"Digite a descrição\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Valor Base</label>\r\n                    <input\r\n                      v-model.number=\"viewModel.valorBase\"\r\n                      class=\"form-control\"\r\n                      type=\"number\"\r\n                      placeholder=\"Digite o valor base\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Tipo Produto</label>\r\n                    <b-form-select\r\n                      v-model=\"viewModel.tipoProdutoId\"\r\n                      :options=\"tiposProdutoOptions\"\r\n                      required\r\n                    ></b-form-select>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <button class=\"btn btn-success mr-2\" type=\"submit\">Salvar</button>\r\n          <button\r\n            class=\"btn btn-secondary\"\r\n            type=\"reset\"\r\n            @click=\"$router.push('/produto')\"\r\n          >\r\n            Voltar\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\n\r\nexport default {\r\n  name: \"NovoProduto\",\r\n  components: {\r\n    RotateSquare\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      tiposProdutoOptions: [],\r\n      viewModel: {\r\n        id: this.$store.state.emptyGuid,\r\n        descricao: \"\",\r\n        valorBase: 0,\r\n        tipoProdutoId: \"\"\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    let produtoId = this.$route.params.id;\r\n    if (produtoId) this.Obter(produtoId);\r\n    this.ObterTiposProdutoSelect();\r\n  },\r\n  methods: {\r\n    ValidarForm(evt) {\r\n      evt.preventDefault();\r\n      if (this.viewModel.id !== this.$store.state.emptyGuid) this.Editar();\r\n      else this.Novo();\r\n    },\r\n    Obter(produtoId) {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"produto/obter/\" + produtoId,\r\n        method: \"GET\"\r\n      })\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.viewModel = resposta.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Novo() {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"produto/novo\",\r\n        data: this.viewModel,\r\n        method: \"POST\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/produto\");\r\n          this.$notify({\r\n            data: [\"Produto cadastrado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Editar() {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"produto/editar\",\r\n        data: this.viewModel,\r\n        method: \"PUT\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/produto\");\r\n          this.$notify({\r\n            data: [\"Produto editado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n\r\n    ObterTiposProdutoSelect() {\r\n      this.$http({\r\n        url: \"/tipoProduto/obter-select\",\r\n        method: \"GET\"\r\n      })\r\n        .then((response) => {\r\n          this.tiposProdutoOptions = response.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Produto"}]}