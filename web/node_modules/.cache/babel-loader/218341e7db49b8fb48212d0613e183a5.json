;AAKA,OAZA;AAaA,KAnDA;AAoDA,IAAA,SApDA,qBAoDA,GApDA,EAoDA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,MAAA,cAAA,CAAA,SAAA,CAAA,GAAA,EAAA,KAAA,YAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,cAAA;AACA,OANA,WAOA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAdA;AAeA,KArEA;AAsEA,IAAA,IAtEA,kBAsEA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA,GAAA,KAAA,YAAA;AACA,MAAA,cAAA,CAAA,IAAA,CAAA,KAAA,SAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,CAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,CAAA,iCAAA,CADA;AAEA,UAAA,IAAA,EAAA,SAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAVA,WAWA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAlBA;AAmBA,KA5FA;AA6FA,IAAA,MA7FA,oBA6FA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,MAAA,cAAA,CAAA,MAAA,CAAA,KAAA,SAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,CAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,CAAA,8BAAA,CADA;AAEA,UAAA,IAAA,EAAA,SAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAVA,WAWA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAlBA;AAmBA,KAlHA;AAmHA,IAAA,MAnHA,oBAmHA;AACA,WAAA,SAAA,CAAA,EAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,SAAA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,IAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,KAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA,GAAA,EAAA;AACA;AAzHA;AAhDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"animated fadeIn\">\r\n    <div v-if=\"loading\" class=\"loading-container\">\r\n      <RotateSquare\r\n        class=\"loading-position animated fadeIn\"\r\n        size=\"60px\"\r\n      ></RotateSquare>\r\n    </div>\r\n    <form v-else @submit=\"ValidarFormDocumento\">\r\n      <div class=\"row\">\r\n        <div class=\"col-sm-12 col-md-12 col-lg-12 col-xl-12\">\r\n          <div class=\"card\">\r\n            <header class=\"card-header\" @click=\"abrir = !abrir\">\r\n              <div class=\"d-flex\">\r\n                <strong class=\"align-self-center\">Contato(s)</strong>\r\n                <small class=\"ml-2 mt-1\">Clique para abrir/esconder</small>\r\n\r\n                <i\r\n                  :class=\"\r\n                    abrir\r\n                      ? 'ml-auto mt-1 fas fa-chevron-up'\r\n                      : 'ml-auto mt-1 fas fa-chevron-down'\r\n                  \"\r\n                ></i>\r\n              </div>\r\n            </header>\r\n            <div :class=\"abrir ? 'collapse-show' : 'collapse'\">\r\n              <div class=\"card-body\">\r\n                <div class=\"row\">\r\n                  <div class=\"col\">\r\n                    <div class=\"form-group\">\r\n                      <small\r\n                        >Campos com * são de preenchimento obrigatório</small\r\n                      >\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                  <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                    <div class=\"form-group\">\r\n                      <label for=\"\">* Nome</label>\r\n                      <input\r\n                        class=\"form-control\"\r\n                        type=\"text\"\r\n                        v-model=\"viewModel.nome\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                    <div class=\"form-group\">\r\n                      <label for=\"\">Telefone</label>\r\n                      <input\r\n                        class=\"form-control\"\r\n                        type=\"text\"\r\n                        v-model=\"viewModel.telefone\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                    <div class=\"form-group\">\r\n                      <label for=\"\">E-mail</label>\r\n                      <input\r\n                        class=\"form-control\"\r\n                        type=\"email\"\r\n                        v-model=\"viewModel.email\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                  <div class=\"col-sm-12 col-md-6 col-lg-6 col-xl-6\">\r\n                    <div class=\"form-group\">\r\n                      <label for>Observação</label>\r\n                      <b-form-textarea\r\n                        v-model=\"viewModel.observacao\"\r\n                        rows=\"4\"\r\n                        max-rows=\"12\"\r\n                        placeholder=\"Observações gerais...\"\r\n                      ></b-form-textarea>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"btn-toolbar mb-3\" role=\"toolbar\">\r\n                  <div class=\"btn-group\" role=\"group\">\r\n                    <button class=\"btn btn-success mr-2\" type=\"submit\">\r\n                      Salvar\r\n                    </button>\r\n                  </div>\r\n                  <div class=\"btn-group\" role=\"group\">\r\n                    <button\r\n                      class=\"btn btn-secondary\"\r\n                      type=\"reset\"\r\n                      @click=\"$router.back()\"\r\n                    >\r\n                      Voltar\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n\r\n                <div class=\"row\">\r\n                  <div class=\"col-12\">\r\n                    <b-table\r\n                      :hover=\"true\"\r\n                      responsive\r\n                      :items=\"itens\"\r\n                      :fields=\"fields\"\r\n                      striped\r\n                      :per-page=\"itensPorPagina\"\r\n                      show-empty\r\n                      empty-text=\"Nenhum contato encontrado.\"\r\n                    >\r\n                      <template v-slot:empty=\"scope\">\r\n                        <h4>{{ scope.emptyText }}</h4>\r\n                      </template>\r\n\r\n                      <template v-slot:cell(acoes)=\"data\">\r\n                        <div class=\"btn-group-sm\">\r\n                          <b-button\r\n                            variant=\"warning\"\r\n                            style=\"margin-right: 10px\"\r\n                            title=\"Editar\"\r\n                            @click=\"Obter(data.item.id)\"\r\n                          >\r\n                            <i class=\"fa fa-edit text-black\"></i>\r\n                          </b-button>\r\n                          <b-button\r\n                            variant=\"danger\"\r\n                            title=\"Remover\"\r\n                            @click=\"Remover(data.item.id)\"\r\n                          >\r\n                            <i class=\"fas fa-trash-alt text-black\"></i>\r\n                          </b-button>\r\n                        </div>\r\n                      </template>\r\n                    </b-table>\r\n                    <b-pagination\r\n                      v-model=\"pagina\"\r\n                      :total-rows=\"total\"\r\n                      :per-page=\"itensPorPagina\"\r\n                      align=\"right\"\r\n                      size=\"md\"\r\n                      class=\"mt-2\"\r\n                    ></b-pagination>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n    <b-modal\r\n      v-model=\"modalRemover\"\r\n      title=\"Confirmar exclusão\"\r\n      class=\"modal-danger\"\r\n      ok-variant=\"danger\"\r\n      @ok=\"ModalOk\"\r\n      @hidden=\"ModalCancel\"\r\n    >\r\n      Você confirma a exclusão desse registro?\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../components/RotateSquare\";\r\nimport ContatoServico from \"../servico/ContatoServico\";\r\n\r\nexport default {\r\n  components: { RotateSquare },\r\n  props: {\r\n    referenciaId: {\r\n      type: String,\r\n      default: \"\"\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      modalRemover: false,\r\n      itemRemover: null,\r\n      loading: false,\r\n      pagina: 1,\r\n      total: 0,\r\n      itensPorPagina: 5,\r\n      itens: [],\r\n      abrir: false,\r\n      fields: [\r\n        { key: \"nome\", label: \"Nome\", sortable: true },\r\n        { key: \"telefone\", label: \"Telefone\", sortable: true },\r\n\r\n        { key: \"email\", label: \"E-mail\", sortable: true },\r\n        {\r\n          key: \"acoes\",\r\n          label: \"Ações\",\r\n          sortable: false,\r\n          thClass: \"center, wd-120-px\"\r\n        }\r\n      ],\r\n      viewModel: {\r\n        id: this.$store.getters.emptyGuid,\r\n        nome: \"\",\r\n        observacao: \"\",\r\n        email: \"\",\r\n        telefone: null,\r\n        referenciaId: \"\"\r\n      }\r\n    };\r\n  },\r\n  mounted() {\r\n    this.ObterGrid(1);\r\n  },\r\n  watch: {\r\n    pagina: function (val) {\r\n      this.ObterGrid(val);\r\n    }\r\n  },\r\n  methods: {\r\n    ModalCancel(evento) {\r\n      evento.preventDefault();\r\n      this.itemRemover = null;\r\n    },\r\n    ModalOk(evento) {\r\n      evento.preventDefault();\r\n      this.modalRemover = false;\r\n      if (!this.itemRemover) return;\r\n\r\n      ContatoServico.Remover(this.itemRemover)\r\n        .then(() => {\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Contato removido com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Remover(item) {\r\n      this.modalRemover = true;\r\n      this.itemRemover = item;\r\n    },\r\n    ValidarFormDocumento(evt) {\r\n      evt.preventDefault();\r\n      if (this.viewModel.id !== this.$store.getters.emptyGuid) this.Editar();\r\n      else this.Novo();\r\n    },\r\n    Obter(id) {\r\n      this.loading = true;\r\n      ContatoServico.Obter(id)\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.viewModel = resposta.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    ObterGrid(val) {\r\n      this.loading = true;\r\n      ContatoServico.ObterGrid(val, this.referenciaId)\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.itens = resposta.data.itens;\r\n          this.total = resposta.data.total;\r\n          this.itensPorPagina = resposta.data.itensPorPagina;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Novo() {\r\n      this.loading = true;\r\n      this.viewModel.referenciaId = this.referenciaId;\r\n      ContatoServico.Novo(this.viewModel)\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.Limpar();\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Contato cadastrado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Editar() {\r\n      this.loading = true;\r\n      ContatoServico.Editar(this.viewModel)\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.Limpar();\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Contato editado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Limpar() {\r\n      this.viewModel.id = this.$store.getters.emptyGuid;\r\n      this.viewModel.telefone = \"\";\r\n      this.viewModel.nome = \"\";\r\n      this.viewModel.email = null;\r\n      this.viewModel.referenciaId = \"\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}