AA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,cAAA;AACA,OAdA,WAeA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAtBA;AAuBA,KAnEA;AAoEA,IAAA,iBApEA,6BAoEA,IApEA,EAoEA;AACA,cAAA,IAAA;AACA,aAAA,0BAAA,CAAA,KAAA;AACA,iBAAA,GAAA;;AACA,aAAA,0BAAA,CAAA,OAAA;AACA,iBAAA,GAAA;;AACA,aAAA,0BAAA,CAAA,aAAA;AACA,iBAAA,GAAA;;AACA,aAAA,0BAAA,CAAA,IAAA;AACA,iBAAA,GAAA;;AACA,aAAA,0BAAA,CAAA,SAAA;AACA,iBAAA,GAAA;;AACA;AACA,iBAAA,UAAA;AAZA;AAcA;AAnFA;AAjDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"animated fadeIn\">\r\n    <div class=\"row\">\r\n      <div class=\"col-sm-12\">\r\n        <div class=\"card\">\r\n          <header class=\"card-header\">\r\n            <div class=\"d-flex\">\r\n              <strong class=\"align-self-center\"\r\n                >Conversão Unidade medida</strong\r\n              >\r\n              <a\r\n                class=\"ml-auto btn btn-primary\"\r\n                href=\"/#/unidadeMedida/novo\"\r\n                title=\"Adicionar nova conversão unidade medida\"\r\n              >\r\n                Adicionar\r\n              </a>\r\n            </div>\r\n          </header>\r\n          <div v-if=\"loading\" class=\"loading-container\">\r\n            <RotateSquare\r\n              class=\"loading-position animated fadeIn\"\r\n              size=\"60px\"\r\n            ></RotateSquare>\r\n          </div>\r\n          <div v-else class=\"card-body\">\r\n            <div class=\"row\">\r\n              <div class=\"col-lg-5 col-md-6 col-sm-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Descrição</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    v-model=\"filtro.descricao\"\r\n                    class=\"form-control\"\r\n                  />\r\n                </div>\r\n              </div>\r\n              <div class=\"col-lg-4 col-md-5 col-sm-12 mt-4\">\r\n                <button\r\n                  class=\"btn btn-primary mr-2\"\r\n                  type=\"button\"\r\n                  @click=\"ObterGrid(1)\"\r\n                >\r\n                  Filtrar\r\n                </button>\r\n                <button\r\n                  class=\"btn btn-secondary\"\r\n                  type=\"button\"\r\n                  @click=\"Limpar()\"\r\n                >\r\n                  Limpar\r\n                </button>\r\n              </div>\r\n            </div>\r\n\r\n            <b-table\r\n              :hover=\"true\"\r\n              responsive\r\n              :items=\"itens\"\r\n              :fields=\"fields\"\r\n              striped\r\n              :per-page=\"itensPorPagina\"\r\n              show-empty\r\n              empty-text=\"Nenhuma conversão unidade medida encontrada.\"\r\n            >\r\n              <template v-slot:empty=\"scope\">\r\n                <h4>{{ scope.emptyText }}</h4>\r\n              </template>\r\n              <template v-slot:cell(acoes)=\"data\">\r\n                <div class=\"btn-group-sm\">\r\n                  <b-button\r\n                    variant=\"warning\"\r\n                    style=\"margin-right: 10px\"\r\n                    title=\"Editar\"\r\n                    @click=\"Editar(data.item)\"\r\n                  >\r\n                    <i class=\"fa fa-edit text-black\"></i>\r\n                  </b-button>\r\n                  <b-button\r\n                    variant=\"danger\"\r\n                    title=\"Remover\"\r\n                    @click=\"Remover(data.item)\"\r\n                  >\r\n                    <i class=\"fas fa-trash-alt text-black\"></i>\r\n                  </b-button>\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(operacao)=\"data\">\r\n                <div class=\"center\">\r\n                  <span>{{ ObterNomeOperacao(data.item.operacao) }}</span>\r\n                </div>\r\n              </template>\r\n            </b-table>\r\n            <b-pagination\r\n              v-model=\"pagina\"\r\n              :total-rows=\"total\"\r\n              :per-page=\"itensPorPagina\"\r\n              align=\"right\"\r\n              size=\"md\"\r\n              class=\"mt-2\"\r\n            ></b-pagination>\r\n            <!-- <b-modal v-model=\"modalShow\">Hello From Modal!</b-modal> -->\r\n            <b-modal\r\n              v-model=\"modalRemover\"\r\n              title=\"Confirmar exclusão\"\r\n              class=\"modal-danger\"\r\n              ok-variant=\"danger\"\r\n              @ok=\"ModalOk\"\r\n              @hidden=\"ModalCancel\"\r\n            >\r\n              Você confirma a exclusão desse registro?\r\n            </b-modal>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport TipoOperacaoAritmeticaEnum from \"../../enums/TipoOperacaoAritmeticaEnum\";\r\n\r\nexport default {\r\n  name: \"UnidadeMedida\",\r\n  components: {\r\n    RotateSquare,\r\n    TipoOperacaoAritmeticaEnum\r\n  },\r\n  data() {\r\n    return {\r\n      modalRemover: false,\r\n      itemRemover: null,\r\n      loading: false,\r\n      itens: [],\r\n      pagina: 1,\r\n      total: 0,\r\n      itensPorPagina: 0,\r\n      filtro: { descricao: \"\" },\r\n      fields: [\r\n        { key: \"descricao\", label: \"Nome\", sortable: true },\r\n        {\r\n          key: \"tipoUnidadeMedida\",\r\n          label: \"Unidade\",\r\n          sortable: true\r\n        },\r\n        // { key: \"operacao\", label: \"Condição\", sortable: true },\r\n        { key: \"valor\", label: \"Quantidade\", sortable: true },\r\n        {\r\n          key: \"tipoUnidadeMedidaBase\",\r\n          label: \"Unidade\",\r\n          sortable: true\r\n        },\r\n        { key: \"observacao\", label: \"Observação\", sortable: true },\r\n\r\n        {\r\n          key: \"acoes\",\r\n          label: \"Ações\",\r\n          sortable: false,\r\n          thClass: \"center, wd-120-px\"\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  watch: {\r\n    pagina: function (val) {\r\n      this.ObterGrid(val);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.ObterGrid(1);\r\n  },\r\n  methods: {\r\n    Limpar() {\r\n      this.filtro.descricao = \"\";\r\n      this.ObterGrid(1);\r\n    },\r\n    Editar(unidadeMedida) {\r\n      console.log(\"/unidadeMedida/editar/\" + unidadeMedida.id);\r\n      this.$router.push(\"/unidadeMedida/editar/\" + unidadeMedida.id);\r\n    },\r\n    ModalCancel(evento) {\r\n      evento.preventDefault();\r\n      this.itemRemover = null;\r\n    },\r\n    ModalOk(evento) {\r\n      evento.preventDefault();\r\n      this.modalRemover = false;\r\n      if (!this.itemRemover) return;\r\n\r\n      this.$http({\r\n        url: \"unidadeMedida/remover/\" + this.itemRemover.id,\r\n        method: \"DELETE\"\r\n      })\r\n        .then(() => {\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Conversão unidade medida removida com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Remover(item) {\r\n      this.modalRemover = true;\r\n      this.itemRemover = item;\r\n    },\r\n    ObterGrid(pagina) {\r\n      this.loading = true;\r\n      this.$http({\r\n        url:\r\n          \"/unidadeMedida/obter-grid?pagina=\" +\r\n          pagina +\r\n          \"&descricao=\" +\r\n          this.filtro.descricao,\r\n        method: \"GET\"\r\n      })\r\n        .then((response) => {\r\n          this.loading = false;\r\n          this.itens = response.data.itens;\r\n          this.total = response.data.total;\r\n          this.pagina = response.data.pagina;\r\n          this.itensPorPagina = response.data.itensPorPagina;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    ObterNomeOperacao(item) {\r\n      switch (item) {\r\n        case TipoOperacaoAritmeticaEnum.Igual:\r\n          return \"=\";\r\n        case TipoOperacaoAritmeticaEnum.Divisao:\r\n          return \"/\";\r\n        case TipoOperacaoAritmeticaEnum.Multiplicacao:\r\n          return \"x\";\r\n        case TipoOperacaoAritmeticaEnum.Soma:\r\n          return \"+\";\r\n        case TipoOperacaoAritmeticaEnum.Subtracao:\r\n          return \"-\";\r\n        default:\r\n          return \"Inválido\";\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/UnidadeMedida"}]}