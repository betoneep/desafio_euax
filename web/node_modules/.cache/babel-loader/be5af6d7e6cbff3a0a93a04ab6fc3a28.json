       <div class=\"row\">\r\n                <div class=\"col\">\r\n                  <div class=\"form-group\">\r\n                    <small>Campos com * são de preenchimento obrigatório</small>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"col-sm-12 col-md-4 col-lg-4 col-xl-4\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Nome</label>\r\n                    <input\r\n                      v-model=\"viewModel.nome\"\r\n                      class=\"form-control\"\r\n                      type=\"text\"\r\n                      placeholder=\"Digite o nome\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                \r\n              </div>\r\n              \r\n            </div>\r\n            <div class=\"btn-toolbar mb-3 ml-3\" role=\"toolbar\">\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button class=\"btn btn-success mr-2\" type=\"submit\">\r\n                  Salvar\r\n                </button>\r\n              </div>\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button\r\n                  class=\"btn btn-secondary\"\r\n                  type=\"reset\"\r\n                  @click=\"$router.go(-1)\"\r\n                >\r\n                  Voltar\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport GrupoUsuarioServico from \"../../servico/GrupoUsuarioServico\";\r\n\r\n\r\nexport default {\r\n  name: \"NovoGrupoUsuario\",\r\n  components: {\r\n    RotateSquare\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      contaOptions: [],\r\n      viewModel: {\r\n        id: this.$store.getters.emptyGuid,\r\n        nome: \"\",\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    let grupo = this.$route.params.id;\r\n    if (grupo) this.Obter(grupo);\r\n  },\r\n  methods: {\r\n    ValidarForm(evt) {\r\n      evt.preventDefault();\r\n      if (this.viewModel.id !== this.$store.getters.emptyGuid) this.Editar();\r\n      else this.Novo();\r\n    },\r\n    Obter(grupoId) {\r\n      this.loading = false;\r\n      GrupoUsuarioServico.Obter(grupoId)\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.viewModel = resposta.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    Novo() {\r\n      this.loading = false;\r\n      GrupoUsuarioServico.Novo(this.viewModel)\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/grupo-usuario\");\r\n          this.$notify({\r\n            data: [\"Grupo cadastro com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    Editar() {\r\n      this.loading = false;\r\n      GrupoUsuarioServico.Editar(this.viewModel)\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/grupo-usuario\");\r\n          this.$notify({\r\n            data: [\"Grupo editado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/GrupoUsuario"}]}