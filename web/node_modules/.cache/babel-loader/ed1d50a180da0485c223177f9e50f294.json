header>\r\n            <div class=\"card-body\">\r\n              <button\r\n                class=\"btn btn-info mr-2\"\r\n                type=\"button\"\r\n                @click=\"toggleAll()\"\r\n              >\r\n                {{ this.descricaoBotao }}\r\n              </button>\r\n              <br /><br />\r\n              <div class=\"row\">\r\n                <div class=\"col-sm-12 col-md-8 col-lg-6 col-xl-6\">\r\n                  <b-table striped responsive :items=\"viewModel.permissoes\">\r\n                    <template v-slot:cell(valor)=\"data\">\r\n                      <div class=\"center\">\r\n                        <span> {{ FormatarNome(data.item.valor) }} </span>\r\n                      </div>\r\n                    </template>\r\n                    <template v-slot:cell(ativo)=\"data\">\r\n                      <div class=\"center\">\r\n                        <b-form-checkbox\r\n                          v-model=\"data.item.ativo\"\r\n                          name=\"check-button\"\r\n                          switch\r\n                        >\r\n                        </b-form-checkbox>\r\n                      </div>\r\n                    </template>\r\n                  </b-table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"btn-toolbar mb-3 ml-3\" role=\"toolbar\">\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button class=\"btn btn-success mr-2\" type=\"submit\">\r\n                  Salvar\r\n                </button>\r\n              </div>\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button\r\n                  class=\"btn btn-secondary\"\r\n                  type=\"reset\"\r\n                  @click=\"$router.go(-1)\"\r\n                >\r\n                  Voltar\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport GrupoUsuarioServico from \"../../servico/GrupoUsuarioServico\";\r\n\r\nexport default {\r\n  name: \"Permissao\",\r\n  components: {\r\n    RotateSquare\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      contaOptions: [],\r\n      descricaoBotao: \"Marcar Todos\",\r\n      toggle: true,\r\n      nomeGrupo: \"\",\r\n      viewModel: {\r\n        permissoes: [],\r\n        nomeGrupo: \"\"\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    let grupo = this.$route.params.id;\r\n    if (grupo) this.Obter(grupo);\r\n  },\r\n  methods: {\r\n    ValidarForm(evt) {\r\n      evt.preventDefault();\r\n      this.Salvar();\r\n    },\r\n    FormatarNome(nome) {\r\n      return nome.replaceAll(\".\", \" / \");\r\n    },\r\n    Salvar() {\r\n      this.loading = false;\r\n      GrupoUsuarioServico.Permissao(this.viewModel)\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: [\"Permiss√£o atualizada com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n          this.$router.go(-1);\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    Obter(grupoId) {\r\n      this.loading = false;\r\n      GrupoUsuarioServico.ObterPermissao(grupoId)\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.viewModel = resposta.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    toggleAll() {\r\n      this.viewModel.permissoes.forEach((permissao) => {\r\n        permissao.ativo = this.toggle;\r\n      });\r\n      this.toggle = !this.toggle;\r\n      this.descricaoBotao = this.toggle ? \"Marcar Todos\" : \"Desmarcar Todos\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/GrupoUsuario"}]}