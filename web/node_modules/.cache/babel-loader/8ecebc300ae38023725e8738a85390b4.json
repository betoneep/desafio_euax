re\";\r\n\r\nexport default {\r\n  name: \"Parametro\",\r\n  components: {\r\n    RotateSquare\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      viewModel: {\r\n        versao: \"\",\r\n        quantidadeUsuario: 0\r\n      }\r\n    };\r\n  },\r\n  computed: {},\r\n  mounted() {\r\n    this.Obter();\r\n  },\r\n  methods: {\r\n    Salvar() {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"parametro/editar\",\r\n        data: this.viewModel,\r\n        method: \"POST\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: [\"Parâmetros salvo com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Obter() {\r\n      this.loading = true;\r\n      this.$http({\r\n        url:\r\n          \"parametro/obter/\" +\r\n          this.$store.getters.getAutenticacao.contaSelecionadaId,\r\n        method: \"GET\"\r\n      })\r\n        .then((resp) => {\r\n          this.viewModel = resp.data;\r\n          this.loading = false;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Parametro"}]}