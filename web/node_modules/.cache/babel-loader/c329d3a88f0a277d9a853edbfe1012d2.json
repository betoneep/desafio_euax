UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAlBA;AAmBA,KAvHA;AAwHA,IAAA,MAxHA,oBAwHA;AACA,WAAA,SAAA,CAAA,EAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,SAAA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,UAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,WAAA,GAAA,CAAA;AACA,WAAA,SAAA,CAAA,gBAAA,GAAA,CAAA;AACA,KA9HA;AA+HA,IAAA,YA/HA,wBA+HA,KA/HA,EA+HA;AACA,UAAA,KAAA,IAAA,IAAA,EAAA;AACA,eAAA,KAAA,CAAA,cAAA,CAAA,OAAA,EAAA;AACA,UAAA,KAAA,EAAA,UADA;AAEA,UAAA,QAAA,EAAA;AAFA,SAAA,CAAA;AAIA,OALA,MAKA;AACA,eAAA,KAAA;AACA;AACA,KAxIA;AAyIA,IAAA,aAzIA,yBAyIA,KAzIA,EAyIA;AACA,UAAA,KAAA,IAAA,IAAA,EAAA;AACA,eAAA,KAAA,CADA,CACA;AACA,OAFA,MAEA;AACA,eAAA,KAAA;AACA;AACA,KA/IA;AAgJA,IAAA,mBAhJA,iCAgJA;AAAA;;AACA,WAAA,KAAA,CAAA;AACA;AACA,QAAA,GAAA,EAAA,sBAFA;AAGA,QAAA,MAAA,EAAA;AAHA,OAAA,EAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAPA,WAQA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAdA;AAeA;AAhKA,qEAiKA,KAjKA,EAiKA;AACA,QAAA,KAAA,IAAA,IAAA,EAAA;AACA,aAAA,KAAA,CAAA,cAAA,CAAA,OAAA,EAAA;AACA,QAAA,KAAA,EAAA,UADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAAA,CAAA;AAIA,KALA,MAKA;AACA,aAAA,KAAA;AACA;AACA,GA1KA,yEA2KA,IA3KA,EA2KA;AACA,YAAA,IAAA;AACA,WAAA,cAAA,CAAA,WAAA;AACA,eAAA,aAAA;;AACA,WAAA,cAAA,CAAA,UAAA;AACA,eAAA,YAAA;;AACA,WAAA,cAAA,CAAA,OAAA;AACA,eAAA,SAAA;;AACA,WAAA,cAAA,CAAA,WAAA;AACA,eAAA,aAAA;;AACA;AACA,eAAA,UAAA;AAVA;AAYA,GAxLA;AAzDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"animated fadeIn\">\r\n    <div v-if=\"loading\" class=\"loading-container\">\r\n      <RotateSquare\r\n        class=\"loading-position animated fadeIn\"\r\n        size=\"60px\"\r\n      ></RotateSquare>\r\n    </div>\r\n    <form v-else @submit=\"ValidarFormContratoCliente\">\r\n      <div class=\"row\">\r\n        <div class=\"col-sm-12 col-md-12 col-lg-12 col-xl-12\">\r\n          <div class=\"card\">\r\n            <header class=\"card-header\" @click=\"abrir = !abrir\">\r\n              <div class=\"d-flex\">\r\n                <strong class=\"align-self-center\">Clientes</strong>\r\n                <small class=\"ml-2 mt-1\">Clique para abrir/esconder</small>\r\n\r\n                <i\r\n                  :class=\"\r\n                    abrir\r\n                      ? 'ml-auto mt-1 fas fa-chevron-up'\r\n                      : 'ml-auto mt-1 fas fa-chevron-down'\r\n                  \"\r\n                ></i>\r\n              </div>\r\n            </header>\r\n            <div :class=\"abrir ? 'collapse-show' : 'collapse'\">\r\n              <div class=\"card-body\">\r\n                <div class=\"row\">\r\n                  <div class=\"col\">\r\n                    <div class=\"form-group\">\r\n                      <small\r\n                        >Campos com * são de preenchimento obrigatório</small\r\n                      >\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                  <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                    <div class=\"form-group\">\r\n                      <label for>* Cliente</label>\r\n                      <b-form-select\r\n                        v-model=\"viewModel.pessoaId\"\r\n                        :options=\"clienteOptions\"\r\n                        required\r\n                      ></b-form-select>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"btn-toolbar mb-3\" role=\"toolbar\">\r\n                  <div class=\"btn-group\" role=\"group\">\r\n                    <button class=\"btn btn-success mr-2\" type=\"submit\">\r\n                      Salvar\r\n                    </button>\r\n                  </div>\r\n                  <div class=\"btn-group\" role=\"group\">\r\n                    <button\r\n                      class=\"btn btn-secondary\"\r\n                      type=\"reset\"\r\n                      @click=\"$router.push('/contrato')\"\r\n                    >\r\n                      Voltar\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n                <div class=\"row\">\r\n                  <div class=\"col-12\">\r\n                    <b-table\r\n                      :hover=\"true\"\r\n                      responsive\r\n                      :items=\"itens\"\r\n                      :fields=\"fields\"\r\n                      striped\r\n                      :per-page=\"itensPorPagina\"\r\n                      show-empty\r\n                      empty-text=\"Nenhum cliente encontrado.\"\r\n                    >\r\n                      <template v-slot:empty=\"scope\">\r\n                        <h4>{{ scope.emptyText }}</h4>\r\n                      </template>\r\n\r\n                      <template v-slot:cell(acoes)=\"data\">\r\n                        <div class=\"btn-group-sm\">\r\n                          <b-button\r\n                            variant=\"warning\"\r\n                            style=\"margin-right: 10px\"\r\n                            title=\"Editar\"\r\n                            @click=\"Obter(data.item.id)\"\r\n                          >\r\n                            <i class=\"fa fa-edit text-black\"></i>\r\n                          </b-button>\r\n                          <b-button\r\n                            variant=\"danger\"\r\n                            title=\"Remover\"\r\n                            @click=\"Remover(data.item.id)\"\r\n                          >\r\n                            <i class=\"fas fa-trash-alt text-black\"></i>\r\n                          </b-button>\r\n                        </div>\r\n                      </template>\r\n                      <template v-slot:cell(tipoPessoa)=\"data\">\r\n                        <div class=\"center\">\r\n                          <span>{{\r\n                            ObterTipoPessoa(data.item.tipoPessoa)\r\n                          }}</span>\r\n                        </div>\r\n                      </template>\r\n                    </b-table>\r\n                    <b-pagination\r\n                      v-model=\"pagina\"\r\n                      :total-rows=\"total\"\r\n                      :per-page=\"itensPorPagina\"\r\n                      align=\"right\"\r\n                      size=\"md\"\r\n                      class=\"mt-2\"\r\n                    ></b-pagination>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n    <b-modal\r\n      v-model=\"modalRemover\"\r\n      title=\"Confirmar exclusão\"\r\n      class=\"modal-danger\"\r\n      ok-variant=\"danger\"\r\n      @ok=\"ModalOk\"\r\n      @hidden=\"ModalCancel\"\r\n    >\r\n      Você confirma a exclusão desse registro?\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport ContratoCliente from \"../../servico/ContratoClienteServico\";\r\nimport TipoPessoaContratoEnum from \"../../enums/TipoPessoaContratoEnum\";\r\nimport TipoPessoaEnum from \"../../enums/TipoPessoaEnum\";\r\n\r\nexport default {\r\n  components: {\r\n    RotateSquare,\r\n    ContratoCliente,\r\n    TipoPessoaContratoEnum,\r\n    TipoPessoaEnum\r\n  },\r\n  props: {\r\n    contratoId: {\r\n      type: String,\r\n      default: \"\"\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      modalRemover: false,\r\n      itemRemover: null,\r\n      clienteOptions: [],\r\n      loading: false,\r\n      pagina: 1,\r\n      total: 0,\r\n      itensPorPagina: 5,\r\n      itens: [],\r\n      abrir: false,\r\n      fields: [\r\n        { key: \"pessoa\", label: \"Cliente\", sortable: true },\r\n        { key: \"tipoPessoa\", label: \"Tipo Pessoa\", sortable: true },\r\n        {\r\n          key: \"acoes\",\r\n          label: \"Ações\",\r\n          sortable: false,\r\n          thClass: \"center, wd-120-px\"\r\n        }\r\n      ],\r\n      viewModel: {\r\n        id: this.$store.getters.emptyGuid,\r\n        pessoaId: \"\",\r\n        contratoId: \"\",\r\n        valorLimite: 0,\r\n        quantidadeLimite: 0,\r\n        tipoPessoaContrato: TipoPessoaContratoEnum.Cliente\r\n      }\r\n    };\r\n  },\r\n  mounted() {\r\n    this.ObterGrid(1);\r\n  },\r\n  watch: {\r\n    pagina: function (val) {\r\n      this.ObterGrid(val);\r\n    }\r\n  },\r\n  created() {\r\n    //let contratoId = this.$route.params.id;\r\n    //if (contratoId) this.Obter(contratoId);\r\n    this.ObterClientesSelect();\r\n  },\r\n  methods: {\r\n    IsNovo() {\r\n      return this.contratoId === this.$store.getters.emptyGuid;\r\n    },\r\n    ValidarFormContratoCliente(evt) {\r\n      evt.preventDefault();\r\n      if (this.viewModel.id !== this.$store.getters.emptyGuid) this.Editar();\r\n      else this.Novo();\r\n    },\r\n    Obter(id) {\r\n      this.loading = true;\r\n      ContratoCliente.Obter(id)\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          //resposta.data.validade = DateTime.formatar(resposta.data.validade);\r\n          this.viewModel = resposta.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    ObterGrid(val) {\r\n      this.loading = true;\r\n      ContratoCliente.ObterGrid(val, this.itensPorPagina, this.contratoId)\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.itens = resposta.data.itens;\r\n          this.total = resposta.data.total;\r\n          this.itensPorPagina = resposta.data.itensPorPagina;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    ModalCancel(evento) {\r\n      evento.preventDefault();\r\n      this.itemRemover = null;\r\n    },\r\n    ModalOk(evento) {\r\n      evento.preventDefault();\r\n      this.modalRemover = false;\r\n      if (!this.itemRemover) return;\r\n\r\n      ContratoCliente.Remover(this.itemRemover)\r\n        .then(() => {\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Cliente removido com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Remover(id) {\r\n      this.modalRemover = true;\r\n      this.itemRemover = id;\r\n    },\r\n    Novo() {\r\n      this.loading = true;\r\n      this.viewModel.contratoId = this.contratoId;\r\n      ContratoCliente.Novo(this.viewModel)\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.Limpar();\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Cliente cadastrado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Editar() {\r\n      this.loading = true;\r\n      this.viewModel.contratoId = this.contratoId;\r\n      ContratoCliente.Editar(this.viewModel)\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.Limpar();\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Cliente editado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Limpar() {\r\n      this.viewModel.id = this.$store.getters.emptyGuid;\r\n      this.viewModel.pessoaId = \"\";\r\n      this.viewModel.contratoId = \"\";\r\n      this.viewModel.valorLimite = 0;\r\n      this.viewModel.quantidadeLimite = 0;\r\n    },\r\n    FormataValor(valor) {\r\n      if (valor != null) {\r\n        return valor.toLocaleString(\"pt-br\", {\r\n          style: \"currency\",\r\n          currency: \"BRL\"\r\n        });\r\n      } else {\r\n        return valor;\r\n      }\r\n    },\r\n    RemoverCifrao(valor) {\r\n      if (valor != null) {\r\n        return valor; //valor.toLocaleString(\"pt-BR\", { minimumFractionDigits: 2 });\r\n      } else {\r\n        return valor;\r\n      }\r\n    },\r\n    ObterClientesSelect() {\r\n      this.$http({\r\n        // url: \"/pessoa/obter-select/\" + TipoPessoaEnum.Fornecedor,\r\n        url: \"/pessoa/obter-select\",\r\n        method: \"GET\"\r\n      })\r\n        .then((response) => {\r\n          this.clienteOptions = response.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    FormataValor(valor) {\r\n      if (valor != null) {\r\n        return valor.toLocaleString(\"pt-br\", {\r\n          style: \"currency\",\r\n          currency: \"BRL\"\r\n        });\r\n      } else {\r\n        return valor;\r\n      }\r\n    },\r\n    ObterTipoPessoa(item) {\r\n      switch (item) {\r\n        case TipoPessoaEnum.Funcionario:\r\n          return \"Funcionário\";\r\n        case TipoPessoaEnum.Fornecedor:\r\n          return \"Fornecedor\";\r\n        case TipoPessoaEnum.Cliente:\r\n          return \"Cliente\";\r\n        case TipoPessoaEnum.Instituicao:\r\n          return \"Instituição\";\r\n        default:\r\n          return \"Inválido\";\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Contrato"}]}