,gBALA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,cAAA;AACA,OAbA,WAcA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OArBA;AAsBA,KAnEA;AAoEA,IAAA,uBApEA,qCAoEA;AAAA;;AACA,WAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,2BADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,mBAAA,GAAA,QAAA,CAAA,IAAA;AACA,OANA,WAOA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAbA;AAcA,KAnFA;AAoFA,IAAA,6BApFA,2CAoFA;AAAA;;AACA,WAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,iCADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,yBAAA,GAAA,QAAA,CAAA,IAAA;AACA,OANA,WAOA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAbA;AAcA,KAnGA;AAoGA,IAAA,YApGA,wBAoGA,KApGA,EAoGA;AACA,UAAA,KAAA,EAAA;AACA,eAAA,KAAA,CAAA,cAAA,CAAA,OAAA,EAAA;AACA,UAAA,KAAA,EAAA,UADA;AAEA,UAAA,QAAA,EAAA;AAFA,SAAA,CAAA;AAIA,OALA,MAKA;AACA,eAAA,GAAA,CAAA,cAAA,CAAA,OAAA,EAAA;AACA,UAAA,KAAA,EAAA,UADA;AAEA,UAAA,QAAA,EAAA;AAFA,SAAA,CAAA;AAIA;AACA,KAhHA;AAiHA,IAAA,wBAjHA,oCAiHA,KAjHA,EAiHA;AACA,aAAA,KAAA,GAAA,KAAA,GAAA,GAAA;AACA,KAnHA;AAoHA,IAAA,wBApHA,oCAoHA,KApHA,EAoHA;AACA,aAAA,KAAA,GAAA,KAAA,GAAA,GAAA,GAAA,GAAA;AACA;AAtHA;AA1DA,CAAA","sourcesContent":["<template>\r\n  <div class=\"animated fadeIn\">\r\n    <div class=\"row\">\r\n      <div class=\"col-sm-12\">\r\n        <div class=\"card\">\r\n          <header class=\"card-header\">\r\n            <div class=\"d-flex\">\r\n              <strong class=\"align-self-center\">Produto</strong>\r\n              <a\r\n                class=\"ml-auto btn btn-primary\"\r\n                href=\"/#/produto/novo\"\r\n                title=\"Adicionar nova produto\"\r\n              >\r\n                Adicionar\r\n              </a>\r\n            </div>\r\n          </header>\r\n          <div v-if=\"loading\" class=\"loading-container\">\r\n            <RotateSquare\r\n              class=\"loading-position animated fadeIn\"\r\n              size=\"60px\"\r\n            ></RotateSquare>\r\n          </div>\r\n          <div v-else class=\"card-body\">\r\n            <div class=\"row\">\r\n              <div class=\"col-lg-2 col-md-6 col-sm-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Descricao</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    v-model=\"filtro.descricao\"\r\n                    class=\"form-control\"\r\n                  />\r\n                </div>\r\n              </div>\r\n              <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                <div class=\"form-group\">\r\n                  <label for>Tipo Produto</label>\r\n                  <b-form-select\r\n                    v-model=\"filtro.tipoProdutoId\"\r\n                    :options=\"tiposProdutoOptions\"\r\n                    required\r\n                  ></b-form-select>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                <div class=\"form-group\">\r\n                  <label for>Unidade Medida</label>\r\n                  <b-form-select\r\n                    v-model=\"filtro.tipoUnidadeMedidaId\"\r\n                    :options=\"tiposUnidadeMedidaOptions\"\r\n                    required\r\n                  ></b-form-select>\r\n                </div>\r\n              </div>\r\n              <div\r\n                class=\"col-sm-6 col-md-2 col-lg-2 col-xl-2\"\r\n                title=\"Presente em pedido.\"\r\n              >\r\n                <label for>Presente em pedido</label>\r\n                <b-form-checkbox\r\n                  v-model=\"filtro.presenteEmPedido\"\r\n                  name=\"check-button\"\r\n                  switch\r\n                >\r\n                </b-form-checkbox>\r\n              </div>\r\n              <div class=\"col-lg-4 col-md-5 col-sm-12 mt-4\">\r\n                <button\r\n                  class=\"btn btn-primary mr-2\"\r\n                  type=\"button\"\r\n                  @click=\"ObterGrid(1)\"\r\n                >\r\n                  Filtrar\r\n                </button>\r\n                <button\r\n                  class=\"btn btn-secondary\"\r\n                  type=\"button\"\r\n                  @click=\"Limpar()\"\r\n                >\r\n                  Limpar\r\n                </button>\r\n              </div>\r\n            </div>\r\n\r\n            <b-table\r\n              :hover=\"true\"\r\n              responsive\r\n              :items=\"itens\"\r\n              :fields=\"fields\"\r\n              striped\r\n              :per-page=\"itensPorPagina\"\r\n              show-empty\r\n              empty-text=\"Nenhum produto encontrado.\"\r\n            >\r\n              <template v-slot:empty=\"scope\">\r\n                <h4>{{ scope.emptyText }}</h4>\r\n              </template>\r\n              <template v-slot:cell(acoes)=\"data\">\r\n                <div class=\"btn-group-sm\">\r\n                  <b-button\r\n                    variant=\"warning\"\r\n                    style=\"margin-right: 10px\"\r\n                    title=\"Editar\"\r\n                    @click=\"Editar(data.item)\"\r\n                  >\r\n                    <i class=\"fa fa-edit\"></i>\r\n                  </b-button>\r\n                  <b-button\r\n                    variant=\"danger\"\r\n                    style=\"margin-right: 10px\"\r\n                    title=\"Remover\"\r\n                    @click=\"Remover(data.item)\"\r\n                  >\r\n                    <i class=\"fas fa-trash-alt\"></i>\r\n                  </b-button>\r\n                  <ModalArquivoGrid :referenciaId=\"data.item.id\" />\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(valorBase)=\"data\">\r\n                <div class=\"left\">\r\n                  <span>{{ FormataValor(data.item.valorBase) }}</span>\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(quantidadeMinimaRateio)=\"data\">\r\n                <div class=\"left\">\r\n                  <span>{{\r\n                    FormataValorMinimoRateio(data.item.quantidadeMinimaRateio)\r\n                  }}</span>\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(margemRateio)=\"data\">\r\n                <div class=\"left\">\r\n                  <span>{{\r\n                    FormataValorMargemRateio(data.item.margemRateio)\r\n                  }}</span>\r\n                </div>\r\n              </template>\r\n            </b-table>\r\n            <b-pagination\r\n              v-model=\"pagina\"\r\n              :total-rows=\"total\"\r\n              :per-page=\"itensPorPagina\"\r\n              align=\"right\"\r\n              size=\"md\"\r\n              class=\"mt-2\"\r\n            ></b-pagination>\r\n            <b-modal\r\n              v-model=\"modalRemover\"\r\n              title=\"Confirmar exclusão\"\r\n              class=\"modal-danger\"\r\n              ok-variant=\"danger\"\r\n              @ok=\"ModalOk\"\r\n              @hidden=\"ModalCancel\"\r\n            >\r\n              Você confirma a exclusão desse registro?\r\n            </b-modal>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport ModalArquivoGrid from \"../../components/ModalArquivoGrid\";\r\nimport ProdutoServico from \"../../servico/ProdutoServico\";\r\n\r\nexport default {\r\n  name: \"Produto\",\r\n  components: {\r\n    RotateSquare,\r\n    ModalArquivoGrid,\r\n    ProdutoServico\r\n  },\r\n  data() {\r\n    return {\r\n      modalRemover: false,\r\n      itemRemover: null,\r\n      loading: false,\r\n      itens: [],\r\n      pagina: 1,\r\n      total: 0,\r\n      itensPorPagina: 20,\r\n      tiposProdutoOptions: [],\r\n      tiposUnidadeMedidaOptions: [],\r\n      filtro: {\r\n        descricao: \"\",\r\n        tipoProdutoId: \"\",\r\n        tipoUnidadeMedidaId: \"\",\r\n        presenteEmPedido: false\r\n      },\r\n      fields: [\r\n        { key: \"descricao\", label: \"Descrição\", sortable: true },\r\n        { key: \"valorBase\", label: \"Valor Base\", sortable: true },\r\n        { key: \"tipoProduto\", label: \"Tipo Produto\", sortable: true },\r\n        {\r\n          key: \"quantidadeMinimaRateio\",\r\n          label: \"Qtd. Miníma Rateio\",\r\n          sortable: true\r\n        },\r\n        {\r\n          key: \"margemRateio\",\r\n          label: \"Margem Rateio\",\r\n          sortable: true\r\n        },\r\n        { key: \"tipoUnidadeMedida\", label: \"Unidade Medida\", sortable: true },\r\n        {\r\n          key: \"acoes\",\r\n          label: \"Ações\",\r\n          sortable: false,\r\n          thClass: \"center, wd-120-px\"\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  watch: {\r\n    pagina: function (val) {\r\n      this.ObterGrid(val);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.ObterGrid(1);\r\n    this.ObterTiposProdutoSelect();\r\n    this.ObterTiposUnidadeMedidaSelect();\r\n  },\r\n  methods: {\r\n    Limpar() {\r\n      this.filtro.descricao = \"\";\r\n      this.filtro.tipoProdutoId = \"\";\r\n      this.filtro.tipoUnidadeMedidaId = \"\";\r\n      this.filtro.presenteEmPedido = false;\r\n    },\r\n    Editar(produto) {\r\n      this.$router.push(\"/produto/editar/\" + produto.id);\r\n    },\r\n    ModalCancel(evento) {\r\n      evento.preventDefault();\r\n      this.itemRemover = null;\r\n    },\r\n    ModalOk(evento) {\r\n      evento.preventDefault();\r\n      this.modalRemover = false;\r\n      if (!this.itemRemover) return;\r\n\r\n      this.$http({\r\n        url: \"produto/remover/\" + this.itemRemover.id,\r\n        method: \"DELETE\"\r\n      })\r\n        .then(() => {\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Produto removida com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    Remover(item) {\r\n      this.modalRemover = true;\r\n      this.itemRemover = item;\r\n    },\r\n    ObterGrid(pagina) {\r\n      this.loading = false;\r\n      ProdutoServico.ObterGrid(\r\n        this.pagina,\r\n        this.filtro.descricao,\r\n        this.filtro.tipoProdutoId,\r\n        this.filtro.tipoUnidadeMedidaId,\r\n        this.filtro.presenteEmPedido\r\n      )\r\n        .then((response) => {\r\n          this.loading = false;\r\n          this.itens = response.data.itens;\r\n          this.total = response.data.total;\r\n          this.pagina = response.data.pagina;\r\n          this.itensPorPagina = response.data.itensPorPagina;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    ObterTiposProdutoSelect() {\r\n      this.$http({\r\n        url: \"/tipoProduto/obter-select\",\r\n        method: \"GET\"\r\n      })\r\n        .then((response) => {\r\n          this.tiposProdutoOptions = response.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    ObterTiposUnidadeMedidaSelect() {\r\n      this.$http({\r\n        url: \"/tipoUnidadeMedida/obter-select\",\r\n        method: \"GET\"\r\n      })\r\n        .then((response) => {\r\n          this.tiposUnidadeMedidaOptions = response.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    FormataValor(valor) {\r\n      if (valor) {\r\n        return valor.toLocaleString(\"pt-br\", {\r\n          style: \"currency\",\r\n          currency: \"BRL\"\r\n        });\r\n      } else {\r\n        return (0.0).toLocaleString(\"pt-br\", {\r\n          style: \"currency\",\r\n          currency: \"BRL\"\r\n        });\r\n      }\r\n    },\r\n    FormataValorMinimoRateio(valor) {\r\n      return valor ? valor : \"-\";\r\n    },\r\n    FormataValorMargemRateio(valor) {\r\n      return valor ? valor + \"%\" : \"-\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Produto"}]}