,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,YAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,CAAA,gCAAA,CADA;AAEA,UAAA,IAAA,EAAA,SAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAbA,WAcA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OArBA;AAsBA,KAhFA;AAkFA,IAAA,uBAlFA,qCAkFA;AAAA;;AACA,WAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,0BAAA,cAAA,CAAA,WADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,kBAAA,GAAA,QAAA,CAAA,IAAA;AACA,OANA,WAOA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAbA;AAcA,KAjGA;AAmGA,IAAA,4BAnGA,0CAmGA;AAAA;;AACA,WAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,+BADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,uBAAA,GAAA,QAAA,CAAA,IAAA;AACA,OANA,WAOA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAbA;AAcA,KAlHA;AAmHA,IAAA,QAnHA,sBAmHA;AACA,aAAA,KAAA,SAAA,CAAA,EAAA,KAAA,KAAA,MAAA,CAAA,OAAA,CAAA,SAAA;AACA;AArHA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"animated fadeIn\">\r\n    <div v-if=\"loading\" class=\"loading-container\">\r\n      <RotateSquare\r\n        class=\"loading-position animated fadeIn\"\r\n        size=\"60px\"\r\n      ></RotateSquare>\r\n    </div>\r\n    <form v-else @submit=\"ValidarForm\">\r\n      <div class=\"row\">\r\n        <div class=\"col-sm-12 col-md-12 col-lg-12 col-xl-12\">\r\n          <div class=\"card\">\r\n            <header class=\"card-header\">\r\n              <strong class=\"align-self-center\"\r\n                >{{\r\n                  viewModel.id == this.$store.getters.emptyGuid\r\n                    ? \"Nova Licitação\"\r\n                    : \"Editar Licitação\"\r\n                }}\r\n              </strong>\r\n            </header>\r\n            <div class=\"card-body\">\r\n              <div class=\"row\">\r\n                <div class=\"col\">\r\n                  <div class=\"form-group\">\r\n                    <small>Campos com * são de preenchimento obrigatório</small>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"col-sm-12 col-md-4 col-lg-4 col-xl-4\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Número</label>\r\n                    <input\r\n                      v-model=\"viewModel.numero\"\r\n                      class=\"form-control\"\r\n                      type=\"text\"\r\n                      placeholder=\"Digite o número\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Instituição</label>\r\n                    <b-form-select\r\n                      v-model=\"viewModel.pessoaId\"\r\n                      :options=\"instituicaoOptions\"\r\n                      required\r\n                    ></b-form-select>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Tipo Instituição</label>\r\n                    <b-form-select\r\n                      v-model=\"viewModel.tipoInstituicaoId\"\r\n                      :options=\"tiposInstituicaoOptions\"\r\n                      required\r\n                    ></b-form-select>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Valor</label>\r\n                    <currency-input\r\n                      v-model=\"viewModel.valor\"\r\n                      class=\"form-control\"\r\n                      placeholder=\"Digite o valor base\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Data Licitação</label>\r\n                    <input\r\n                      v-model=\"viewModel.dataLicitacao\"\r\n                      class=\"form-control\"\r\n                      type=\"date\"\r\n                      placeholder=\"Digite a data da licitação\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Data de Vencimento</label>\r\n                    <input\r\n                      v-model=\"viewModel.dataVencimento\"\r\n                      class=\"form-control\"\r\n                      type=\"date\"\r\n                      placeholder=\"Digite a data de vencimento\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-4 col-lg-4 col-xl-4\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Local</label>\r\n                    <input\r\n                      v-model=\"viewModel.local\"\r\n                      class=\"form-control\"\r\n                      type=\"text\"\r\n                      placeholder=\"Digite o local\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-4 col-lg-4 col-xl-4\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Regimento</label>\r\n                    <input\r\n                      v-model=\"viewModel.regimento\"\r\n                      class=\"form-control\"\r\n                      type=\"text\"\r\n                      placeholder=\"Digite o regimento\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"btn-toolbar mb-3 ml-3\" role=\"toolbar\">\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button class=\"btn btn-success mr-2\" type=\"submit\">\r\n                  Salvar\r\n                </button>\r\n              </div>\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button\r\n                  class=\"btn btn-secondary\"\r\n                  type=\"reset\"\r\n                  @click=\"$router.push('/licitacao')\"\r\n                >\r\n                  Voltar\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n    <div v-if=\"IsEdicao()\">\r\n      <LicitacaoContrato :licitacaoId=\"viewModel.id\"> </LicitacaoContrato>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport TipoPessoaEnum from \"../../enums/TipoPessoaEnum\";\r\nimport DateTime from \"../../util/DateTime\";\r\nimport LicitacaoContrato from \"./LicitacaoContrato\";\r\n\r\nexport default {\r\n  name: \"NovoLicitacao\",\r\n  components: {\r\n    RotateSquare,\r\n    TipoPessoaEnum,\r\n    LicitacaoContrato\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      tiposInstituicaoOptions: [],\r\n      instituicaoOptions: [],\r\n      viewModel: {\r\n        id: this.$store.getters.emptyGuid,\r\n        pessoaId: \"\",\r\n        tipoInstituicaoId: \"\",\r\n        valor: 0,\r\n        local: \"\",\r\n        regimento: \"\",\r\n        numero: \"\",\r\n        dataLicitacao: \"\",\r\n        dataVencimento: \"\",\r\n        contratos: []\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    let licitacaoId = this.$route.params.id;\r\n    if (licitacaoId) this.Obter(licitacaoId);\r\n    this.ObterInstituicoesSelect();\r\n    this.ObterTiposInstituicoesSelect();\r\n  },\r\n  methods: {\r\n    ValidarForm(evt) {\r\n      evt.preventDefault();\r\n      if (this.viewModel.id !== this.$store.getters.emptyGuid) this.Editar();\r\n      else this.Novo();\r\n    },\r\n    Obter(licitacaoId) {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"licitacao/obter/\" + licitacaoId,\r\n        method: \"GET\"\r\n      })\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          resposta.data.dataLicitacao = DateTime.formatar(\r\n            resposta.data.dataLicitacao\r\n          );\r\n          resposta.data.dataVencimento = DateTime.formatar(\r\n            resposta.data.dataVencimento\r\n          );\r\n          this.viewModel = resposta.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Novo() {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"licitacao/novo\",\r\n        data: this.viewModel,\r\n        method: \"POST\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/licitacao\");\r\n          this.$notify({\r\n            data: [\"Licitacao cadastrada com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Editar() {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"licitacao/editar\",\r\n        data: this.viewModel,\r\n        method: \"PUT\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/licitacao\");\r\n          this.$notify({\r\n            data: [\"Licitação editada com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n\r\n    ObterInstituicoesSelect() {\r\n      this.$http({\r\n        url: \"/pessoa/obter-select/\" + TipoPessoaEnum.Instituicao,\r\n        method: \"GET\"\r\n      })\r\n        .then((response) => {\r\n          this.instituicaoOptions = response.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n\r\n    ObterTiposInstituicoesSelect() {\r\n      this.$http({\r\n        url: \"/tipoInstituicao/obter-select\",\r\n        method: \"GET\"\r\n      })\r\n        .then((response) => {\r\n          this.tiposInstituicaoOptions = response.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    IsEdicao() {\r\n      return this.viewModel.id !== this.$store.getters.emptyGuid;\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Licitacao"}]}