KAAA,cAFA,EAGA,KAAA,MAAA,CAAA,IAHA,EAIA,KAAA,MAAA,CAAA,WAJA,EAKA,KAAA,MAAA,CAAA,SALA,EAMA,KAAA,MAAA,CAAA,eANA,EAOA,KAAA,MAAA,CAAA,iBAPA,EASA,IATA,CASA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,cAAA;AACA,OAfA,WAgBA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAvBA;AAwBA,KArEA;AAsEA,IAAA,YAtEA,wBAsEA,KAtEA,EAsEA;AACA,aAAA,KAAA,GAAA,IAAA,IAAA,CAAA,KAAA,EAAA,kBAAA,EAAA,GAAA,EAAA;AACA,KAxEA;AAyEA,IAAA,iBAzEA,6BAyEA,KAzEA,EAyEA;AACA,aAAA,KAAA,GACA,IAAA,CAAA,KAAA,CAAA,CAAA,KAAA,GAAA,MAAA,CAAA,OAAA,IAAA,GAAA,IAAA,GAAA,GAAA,GADA,GAEA,GAFA;AAGA;AA7EA;AA/CA,CAAA","sourcesContent":["<template>\r\n  <div class=\"animated fadeIn\">\r\n    <div class=\"row\">\r\n      <div class=\"col-sm-12\">\r\n        <div class=\"card\">\r\n          <header class=\"card-header\">\r\n            <div class=\"d-flex\">\r\n              <strong class=\"align-self-center\">Projetos</strong>\r\n              <a\r\n                class=\"ml-auto btn btn-primary\"\r\n                href=\"/#/projeto/novo\"\r\n                title=\"Adicionar novo projeto\"\r\n              >\r\n                Novo Projeto\r\n              </a>\r\n            </div>\r\n          </header>\r\n          <div v-if=\"loading\" class=\"loading-container\">\r\n            <RotateSquare\r\n              class=\"loading-position animated fadeIn\"\r\n              size=\"60px\"\r\n            ></RotateSquare>\r\n          </div>\r\n          <div v-else class=\"card-body\">\r\n            <div class=\"row\">\r\n              <div class=\"col-lg-4 col-md-6 col-sm-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Nome</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    v-model=\"filtro.nome\"\r\n                    class=\"form-control\"\r\n                  />\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                <div class=\"form-group\">\r\n                  <label for>Data Inicial</label>\r\n                  <input\r\n                    v-model=\"filtro.dataInicial\"\r\n                    class=\"form-control\"\r\n                    type=\"date\"\r\n                    placeholder=\"Digite a data inicial\"\r\n                  />\r\n                </div>\r\n              </div>\r\n              <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                <div class=\"form-group\">\r\n                  <label for>Data Final</label>\r\n                  <input\r\n                    v-model=\"filtro.dataFinal\"\r\n                    class=\"form-control\"\r\n                    type=\"date\"\r\n                    placeholder=\"Digite a data final\"\r\n                  />\r\n                </div>\r\n              </div>\r\n              <div\r\n                class=\"col-sm-6 col-md-2 col-lg-2 col-xl-1\"\r\n                title=\"Apenas projetos atrasados.\"\r\n              >\r\n                <label for>Atrasados</label>\r\n                <b-form-checkbox\r\n                  v-model=\"filtro.projetoAtrasado\"\r\n                  name=\"check-button\"\r\n                  switch\r\n                >\r\n                </b-form-checkbox>\r\n              </div>\r\n              <div\r\n                class=\"col-sm-6 col-md-2 col-lg-2 col-xl-1\"\r\n                title=\"Apenas projetos finalizados.\"\r\n              >\r\n                <label for>Finalizados</label>\r\n                <b-form-checkbox\r\n                  v-model=\"filtro.projetoFinalizado\"\r\n                  name=\"check-button\"\r\n                  switch\r\n                >\r\n                </b-form-checkbox>\r\n              </div>\r\n              <div class=\"col-lg-4 col-md-5 col-sm-12 mt-4\">\r\n                <button\r\n                  class=\"btn btn-primary mr-2\"\r\n                  type=\"button\"\r\n                  @click=\"ObterGrid(1)\"\r\n                >\r\n                  Filtrar\r\n                </button>\r\n                <button\r\n                  class=\"btn btn-secondary\"\r\n                  type=\"button\"\r\n                  @click=\"Limpar()\"\r\n                >\r\n                  Limpar\r\n                </button>\r\n              </div>\r\n            </div>\r\n\r\n            <b-table\r\n              :hover=\"true\"\r\n              responsive\r\n              :items=\"itens\"\r\n              :fields=\"fields\"\r\n              striped\r\n              :per-page=\"itensPorPagina\"\r\n              show-empty\r\n              empty-text=\"Nenhum projeto encontrado.\"\r\n            >\r\n              <template v-slot:empty=\"scope\">\r\n                <h4>{{ scope.emptyText }}</h4>\r\n              </template>\r\n              <template v-slot:cell(acoes)=\"data\">\r\n                <div class=\"btn-group-sm\">\r\n                  <b-button\r\n                    variant=\"warning\"\r\n                    style=\"margin-right: 10px\"\r\n                    title=\"Editar\"\r\n                    @click=\"Editar(data.item)\"\r\n                  >\r\n                    <i class=\"fa fa-edit\"></i>\r\n                  </b-button>\r\n                  <b-button\r\n                    variant=\"danger\"\r\n                    style=\"margin-right: 10px\"\r\n                    title=\"Remover\"\r\n                    @click=\"Remover(data.item)\"\r\n                  >\r\n                    <i class=\"fas fa-trash-alt\"></i>\r\n                  </b-button>\r\n                  <ModalArquivoGrid :referenciaId=\"data.item.id\" />\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(dataInicio)=\"data\">\r\n                <div class=\"center\">\r\n                  <span>{{ FormatarData(data.item.dataInicio) }}</span>\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(dataFim)=\"data\">\r\n                <div class=\"center\">\r\n                  <span>{{ FormatarData(data.item.dataFim) }}</span>\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(finalizado)=\"data\">\r\n                <div class=\"center\">\r\n                  <span v-if=\"data.item.finalizado\" class=\"badge badge-success\">\r\n                    Sim\r\n                  </span>\r\n                  <span v-else class=\"badge badge-danger\">Não</span>\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(atrasado)=\"data\">\r\n                <div class=\"center\">\r\n                  <span v-if=\"data.item.atrasado\" class=\"badge badge-danger\">\r\n                    Sim\r\n                  </span>\r\n                  <span v-else class=\"badge badge-success\">Não</span>\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(percentual)=\"data\">\r\n                <div class=\"left\">\r\n                  <span>{{ FormataPercentual(data.item.percentual) }}</span>\r\n                </div>\r\n              </template>\r\n            </b-table>\r\n            <b-pagination\r\n              v-model=\"pagina\"\r\n              :total-rows=\"total\"\r\n              :per-page=\"itensPorPagina\"\r\n              align=\"right\"\r\n              size=\"md\"\r\n              class=\"mt-2\"\r\n            ></b-pagination>\r\n            <b-modal\r\n              v-model=\"modalRemover\"\r\n              title=\"Confirmar exclusão do projeto\"\r\n              class=\"modal-danger\"\r\n              ok-variant=\"danger\"\r\n              @ok=\"ModalOk\"\r\n              @hidden=\"ModalCancel\"\r\n            >\r\n              Você confirma a exclusão desse projeto?\r\n            </b-modal>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport ModalArquivoGrid from \"../../components/ModalArquivoGrid\";\r\nimport ProjetoServico from \"../../servico/ProjetoServico\";\r\n\r\nexport default {\r\n  name: \"Projeto\",\r\n  components: {\r\n    RotateSquare,\r\n    ModalArquivoGrid,\r\n    ProjetoServico\r\n  },\r\n  data() {\r\n    return {\r\n      modalRemover: false,\r\n      itemRemover: null,\r\n      loading: false,\r\n      itens: [],\r\n      pagina: 1,\r\n      total: 0,\r\n      itensPorPagina: 20,\r\n      filtro: {\r\n        nome: \"\",\r\n        dataInicial: \"\",\r\n        dataFinal: \"\",\r\n        projetoAtrasado: false,\r\n        projetoFinalizado: false\r\n      },\r\n      fields: [\r\n        { key: \"nome\", label: \"Nome\", sortable: true },\r\n        { key: \"dataInicio\", label: \"Data Inicial\", sortable: true },\r\n        { key: \"dataFim\", label: \"Data Final\", sortable: true },\r\n        { key: \"percentual\", label: \"% Completo\", sortable: true },\r\n        { key: \"atrasado\", label: \"Atrasado\", sortable: true },\r\n        { key: \"finalizado\", label: \"Finalizado\", sortable: true },\r\n        {\r\n          key: \"acoes\",\r\n          label: \"Ações\",\r\n          sortable: false,\r\n          thClass: \"center, wd-120-px\"\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  watch: {\r\n    pagina: function (val) {\r\n      this.ObterGrid(val);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.ObterGrid(1);\r\n  },\r\n  methods: {\r\n    Limpar() {\r\n      this.filtro.nome = \"\";\r\n      this.filtro.dataInicial = \"\";\r\n      this.filtro.dataFinal = \"\";\r\n      this.filtro.projetoAtrasado = false;\r\n      this.filtro.projetoFinalizado = false;\r\n      this.ObterGrid(this.pagina);\r\n    },\r\n    Editar(projeto) {\r\n      this.$router.push(\"/projeto/editar/\" + projeto.id);\r\n    },\r\n    ModalCancel(evento) {\r\n      evento.preventDefault();\r\n      this.itemRemover = null;\r\n    },\r\n    ModalOk(evento) {\r\n      evento.preventDefault();\r\n      this.modalRemover = false;\r\n      if (!this.itemRemover) return;\r\n\r\n      ProjetoServico.Remover(this.itemRemover.id)\r\n        .then(() => {\r\n          this.ObterGrid(1);\r\n          this.$notify({\r\n            data: [\"Projeto removido com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    Remover(item) {\r\n      this.modalRemover = true;\r\n      this.itemRemover = item;\r\n    },\r\n    ObterGrid(pagina) {\r\n      this.loading = false;\r\n\r\n      ProjetoServico.ObterGrid(\r\n        pagina,\r\n        this.itensPorPagina,\r\n        this.filtro.nome,\r\n        this.filtro.dataInicial,\r\n        this.filtro.dataFinal,\r\n        this.filtro.projetoAtrasado,\r\n        this.filtro.projetoFinalizado\r\n      )\r\n        .then((response) => {\r\n          this.loading = false;\r\n          this.itens = response.data.itens;\r\n          this.total = response.data.total;\r\n          this.pagina = response.data.pagina;\r\n          this.itensPorPagina = response.data.itensPorPagina;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    FormatarData(value) {\r\n      return value ? new Date(value).toLocaleDateString() : \"\";\r\n    },\r\n    FormataPercentual(valor) {\r\n      return valor\r\n        ? Math.round((valor + Number.EPSILON) * 100) / 100 + \"%\"\r\n        : \"-\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Projeto"}]}