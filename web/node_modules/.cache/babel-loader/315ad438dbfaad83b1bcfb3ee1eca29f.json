AAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,eADA;AAEA,QAAA,IAAA,EAAA,KAAA,SAFA;AAGA,QAAA,MAAA,EAAA;AAHA,OAAA,EAKA,IALA,CAKA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,eAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,CAAA,6BAAA,CADA;AAEA,UAAA,IAAA,EAAA,SAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAbA,WAcA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OArBA;AAsBA,KAtDA;AAuDA,IAAA,eAvDA,6BAuDA;AACA,WAAA,KAAA,CAAA,KAAA,SAAA,CAAA,EAAA;AACA,KAzDA;AA0DA,IAAA,0BA1DA,wCA0DA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,KA5DA;AA6DA,IAAA,iBA7DA,6BA6DA,MA7DA,EA6DA;AACA,MAAA,MAAA,CAAA,cAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,KAhEA;AAiEA,IAAA,aAjEA,yBAiEA,MAjEA,EAiEA;AAAA;;AACA,MAAA,MAAA,CAAA,cAAA;AACA,WAAA,WAAA,GAAA,KAAA;AAEA,MAAA,aAAA,CAAA,aAAA,CAAA,KAAA,SAAA,CAAA,QAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,EAAA,GAAA,QAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA;;AACA,QAAA,GAAA,CAAA,KAAA,CAAA,8BAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,0BAAA,QAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,CAAA,+BAAA,CADA;AAEA,UAAA,IAAA,EAAA,SAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAXA,WAYA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAlBA;AAmBA;AAxFA,mDAyFA;AACA,SAAA,KAAA,CAAA,KAAA,SAAA,CAAA,EAAA;AACA,GA3FA;AAtDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"animated fadeIn\">\r\n    <div v-if=\"loading\" class=\"loading-container\">\r\n      <RotateSquare\r\n        class=\"loading-position animated fadeIn\"\r\n        size=\"60px\"\r\n      ></RotateSquare>\r\n    </div>\r\n    <form v-else @submit=\"ValidarForm\">\r\n      <div class=\"row\">\r\n        <div class=\"col-sm-12 col-md-12 col-lg-12 col-xl-12\">\r\n          <div class=\"card\">\r\n            <header class=\"card-header\">\r\n              <div class=\"d-flex\">\r\n                <strong class=\"align-self-center\">Rateio</strong>\r\n                <a\r\n                  @click=\"RecalcularRateioAutomatico()\"\r\n                  class=\"ml-auto btn btn-danger\"\r\n                  href=\"#\"\r\n                  title=\"Recalcular rateio automático\"\r\n                >\r\n                  Recalcular Rateio Automático\r\n                </a>\r\n              </div>\r\n            </header>\r\n            <div class=\"card-body\">\r\n              <div class=\"row\">\r\n                <div class=\"col-sm-12 col-md-4 col-lg-4 col-xl-6\">\r\n                  <div class=\"form-group\">\r\n                    <label for>Pedido</label>\r\n                    <input\r\n                      bold\r\n                      disabled\r\n                      v-model=\"viewModel.pedido\"\r\n                      class=\"form-control\"\r\n                      type=\"text\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                  <div class=\"form-group\">\r\n                    <label for>Previsão Entrega</label>\r\n                    <input\r\n                      disabled\r\n                      v-model=\"viewModel.previsaEntrega\"\r\n                      class=\"form-control\"\r\n                      type=\"date\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-3\">\r\n                  <div class=\"form-group\">\r\n                    <label for>Status Pedido</label>\r\n                    <b-form-select\r\n                      disabled\r\n                      v-model=\"viewModel.statusPedido\"\r\n                      :options=\"statusPedidoOptions\"\r\n                    ></b-form-select>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                  <div class=\"form-group\">\r\n                    <label for>Data Rateio</label>\r\n                    <input\r\n                      disabled\r\n                      v-model=\"viewModel.dataRateio\"\r\n                      class=\"form-control\"\r\n                      type=\"date\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                  <div class=\"form-group\">\r\n                    <label for>Status Rateio</label>\r\n                    <b-form-select\r\n                      disabled\r\n                      v-model=\"viewModel.status\"\r\n                      :options=\"statusRateioOptions\"\r\n                    ></b-form-select>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                  <div class=\"form-group\">\r\n                    <label for>Valor Pedido</label>\r\n                    <currency-input\r\n                      disabled\r\n                      v-model=\"viewModel.valorPedido\"\r\n                      class=\"form-control\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                  <div class=\"form-group\">\r\n                    <label for>Valor Rateado</label>\r\n                    <currency-input\r\n                      disabled\r\n                      v-model=\"viewModel.valorRateado\"\r\n                      class=\"form-control\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-12 col-md-3 col-lg-3 col-xl-2\">\r\n                  <div class=\"form-group\">\r\n                    <label for>Rateio Automático</label>\r\n                    <input\r\n                      disabled\r\n                      v-model=\"viewModel.automatico\"\r\n                      class=\"form-control\"\r\n                      type=\"text\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"col-sm-12 col-md-6 col-lg-6 col-xl-6\">\r\n                  <div class=\"form-group\">\r\n                    <label for>Observação</label>\r\n                    <b-form-textarea\r\n                      v-model=\"viewModel.observacao\"\r\n                      rows=\"4\"\r\n                      max-rows=\"12\"\r\n                      placeholder=\"Observações gerais...\"\r\n                    ></b-form-textarea>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"btn-toolbar mb-3 ml-3\" role=\"toolbar\">\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button class=\"btn btn-success mr-2\" type=\"submit\">\r\n                  Salvar\r\n                </button>\r\n              </div>\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button\r\n                  class=\"btn btn-secondary\"\r\n                  type=\"reset\"\r\n                  @click=\"$router.go(-1)\"\r\n                >\r\n                  Voltar\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n    <b-modal\r\n      v-model=\"modalRateio\"\r\n      title=\"Confirmar execução rateio\"\r\n      class=\"modal-danger\"\r\n      ok-variant=\"info\"\r\n      @ok=\"ModalRateioOk\"\r\n      @hidden=\"ModalRateioCancel\"\r\n    >\r\n      Você confirma a execução do rateio para este pedido?\r\n    </b-modal>\r\n    <div>\r\n      <RateioFornecedor\r\n        :rateioId=\"this.viewModel.id\"\r\n        @atualizarRateio=\"AtualizarRateio\"\r\n      >\r\n      </RateioFornecedor>\r\n      <!-- <PedidoProduto :pedidoId=\"viewModel.id\"> </PedidoProduto> -->\r\n      <NovoDocumento :referenciaId=\"this.viewModel.id\"> </NovoDocumento>\r\n      <Contato :referenciaId=\"this.viewModel.id\"> </Contato>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport DateTime from \"../../util/DateTime\";\r\nimport NovoDocumento from \"../../components/NovoDocumento\";\r\nimport StatusPedidoEnum from \"../../enums/StatusPedidoEnum\";\r\nimport StatusRateioEnum from \"../../enums/StatusRateioEnum\";\r\nimport RateioFornecedor from \"./RateioFornecedor\";\r\nimport RateioServico from \"../../servico/RateioServico\";\r\n\r\n// import PedidoProduto from \"./PedidoProduto\";\r\nimport Contato from \"../../components/Contato\";\r\nimport Bus from \"../../util/EventBus\";\r\n\r\nexport default {\r\n  name: \"DetalheRateioPedido\",\r\n  components: {\r\n    Bus,\r\n    RotateSquare,\r\n    NovoDocumento,\r\n    StatusPedidoEnum,\r\n    StatusRateioEnum,\r\n    Contato,\r\n    RateioFornecedor,\r\n    RateioServico\r\n    // PedidoProduto,\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      modalRateio: false,\r\n      statusPedidoOptions: [\r\n        { value: StatusPedidoEnum.Pendente, text: \"Pendente\" },\r\n        { value: StatusPedidoEnum.Aberto, text: \"Aberto\" },\r\n        {\r\n          value: StatusPedidoEnum.AguardandoProdutos,\r\n          text: \"Aguardando Produtos\"\r\n        },\r\n        { value: StatusPedidoEnum.Incompleto, text: \"Incompleto\" },\r\n        { value: StatusPedidoEnum.EmRota, text: \"Em Rota\" },\r\n        { value: StatusPedidoEnum.Entregue, text: \"Entregue\" },\r\n        { value: StatusPedidoEnum.Finalizado, text: \"Finalizado\" },\r\n        { value: StatusPedidoEnum.Cancelado, text: \"Cancelado\" }\r\n      ],\r\n      statusRateioOptions: [\r\n        { value: StatusRateioEnum.Pendente, text: \"Pendente\" },\r\n        { value: StatusRateioEnum.Incompleto, text: \"Finalizado\" },\r\n        { value: StatusRateioEnum.Completo, text: \"Cancelado\" },\r\n        { value: StatusRateioEnum.Cancelada, text: \"Aberto\" }\r\n      ],\r\n      tiposInstituicaoOptions: [],\r\n      licitacaoOptions: [],\r\n      contratoOptions: [],\r\n      viewModel: {\r\n        id: this.$store.getters.emptyGuid,\r\n        descricao: \"\"\r\n      }\r\n    };\r\n  },\r\n  watch: {},\r\n  created() {\r\n    this.viewModel.id = this.$route.params.id;\r\n    this.Obter(this.$route.params.id);\r\n\r\n    // Bus.$on(\"remocao-produto-pedido\", () => {\r\n    //   this.AtualizarRateio();\r\n    // });\r\n  },\r\n  methods: {\r\n    ValidarForm(evt) {\r\n      evt.preventDefault();\r\n      this.Editar();\r\n    },\r\n    Obter(rateioId) {\r\n      this.loading = false;\r\n      this.$http({\r\n        url: \"rateio/obter/\" + rateioId,\r\n        method: \"GET\"\r\n      })\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          resposta.data.previsaEntrega = DateTime.formatar(\r\n            resposta.data.previsaEntrega\r\n          );\r\n          resposta.data.dataRateio = DateTime.formatar(\r\n            resposta.data.dataRateio\r\n          );\r\n          this.viewModel = resposta.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    Editar() {\r\n      this.loading = false;\r\n      this.$http({\r\n        url: \"rateio/editar\",\r\n        data: this.viewModel,\r\n        method: \"PUT\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/rateiopedido\");\r\n          this.$notify({\r\n            data: [\"Rateio editado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    AtualizarRateio() {\r\n      this.Obter(this.viewModel.id);\r\n    },\r\n    RecalcularRateioAutomatico() {\r\n      this.modalRateio = true;\r\n    },\r\n    ModalRateioCancel(evento) {\r\n      evento.preventDefault();\r\n      this.modalRateio = false;\r\n    },\r\n    ModalRateioOk(evento) {\r\n      evento.preventDefault();\r\n      this.modalRateio = false;\r\n\r\n      RateioServico.EfetuarRateio(this.viewModel.pedidoId)\r\n        .then((resposta) => {\r\n          this.viewModel.id = resposta.data;\r\n          this.Obter(resposta.data);\r\n          Bus.$emit(\"atualiza-fornecedores-rateio\");\r\n          this.$router.push(\"/rateiopedido/editar/\" + resposta.data);\r\n          this.$notify({\r\n            data: [\"Rateio executado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    AtualizarRateio() {\r\n      this.Obter(this.viewModel.id);\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/RateioPedido"}]}