A,MAAA,EAAA,gBAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,GAAA;AACA;AAHA,GA1BA;AAgCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,UAJA,wBAIA;AACA,WAAA,KAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,KAAA,MAAA;AACA,KARA;AASA,IAAA,IATA,kBASA;AAAA;;AACA,WAAA,OAAA,GAAA,KAAA;AACA,MAAA,kBAAA,CAAA,IAAA,CAAA,KAAA,SAAA,CAAA,EAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,SAAA,CAAA,KAAA,CAAA,MAAA;AACA,OALA,WAMA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAbA;AAcA,KAzBA;AA0BA,IAAA,SA1BA,qBA0BA,MA1BA,EA0BA;AAAA;;AACA,WAAA,OAAA,GAAA,KAAA;AACA,MAAA,kBAAA,CAAA,SAAA,CAAA,MAAA,EAAA,KAAA,cAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,cAAA;AACA,OAPA,WAQA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,UAAA,IAAA,EAAA,MAFA;AAGA,UAAA,QAAA,EAAA;AAHA,SAAA;AAKA,OAfA;AAgBA;AA5CA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <a class=\"btn\" @click=\"AbrirModal()\" title=\"Visualizar notificações\">\r\n    <i class=\"fas fa-bell\"></i>\r\n    <b-modal\r\n      v-model=\"abrir\"\r\n      title=\"Notificações\"\r\n      size=\"lg\"\r\n      scrollable\r\n      hide-footer\r\n    >\r\n      <div v-if=\"loading\" class=\"loading-container\">\r\n        <RotateSquare\r\n          class=\"loading-position animated fadeIn\"\r\n          size=\"60px\"\r\n        ></RotateSquare>\r\n      </div>\r\n      <div v-else>\r\n        <div v-if=\"viewModel\">\r\n          <div class=\"row\">\r\n            <div class=\"col-6\">\r\n              <label for=\"\"\r\n                >Data: {{ new Date(viewModel.data).toLocaleString() }}</label\r\n              >\r\n            </div>\r\n            <div class=\"col-6\" v-if=\"viewModel.url\">\r\n              <div class=\"form-group\">\r\n                <label class=\"mr-2\" for>Link:</label>\r\n                <a :href=\"viewModel.url\" target=\"_blank\">{{ viewModel.url }}</a>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"row\">\r\n            <div class=\"col-6\">\r\n              <div class=\"form-group\">\r\n                <label for>Mensagem</label>\r\n                <b-form-textarea\r\n                  v-model=\"viewModel.mensagem\"\r\n                  rows=\"4\"\r\n                  max-rows=\"12\"\r\n                  readonly\r\n                ></b-form-textarea>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-6 mt-4\" v-if=\"viewModel.lido\">\r\n              <button class=\"btn btn-danger\" @click=\"Lido()\">\r\n                Marcar não lido\r\n              </button>\r\n            </div>\r\n            <div class=\"col-6 mt-4\" v-else>\r\n              <button class=\"btn btn-success\" @click=\"Lido()\">\r\n                Marcar como lido\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <b-table\r\n              :hover=\"true\"\r\n              responsive\r\n              :items=\"itens\"\r\n              :fields=\"fields\"\r\n              striped\r\n              :per-page=\"itensPorPagina\"\r\n              show-empty\r\n              empty-text=\"Nenhuma notificação.\"\r\n            >\r\n              <template v-slot:empty=\"scope\">\r\n                <h4>{{ scope.emptyText }}</h4>\r\n              </template>\r\n              <template v-slot:cell(lido)=\"data\">\r\n                <div class=\"center\">\r\n                  <span v-if=\"data.item.lido\" class=\"badge badge-success\">\r\n                    Sim\r\n                  </span>\r\n                  <span v-else class=\"badge badge-secondary\">Não</span>\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(data)=\"data\">\r\n                <div class=\"center\">\r\n                  <span>\r\n                    {{ new Date(data.item.data).toLocaleString() }}\r\n                  </span>\r\n                </div>\r\n              </template>\r\n              <template v-slot:cell(acoes)=\"data\">\r\n                <div class=\"btn-group-sm\">\r\n                  <b-button\r\n                    variant=\"warning\"\r\n                    style=\"margin-right: 10px\"\r\n                    title=\"Visualizar notificação\"\r\n                    @click=\"Visualizar(data.item)\"\r\n                  >\r\n                    <i class=\"fas fa-eye\"></i>\r\n                  </b-button>\r\n                </div>\r\n              </template>\r\n            </b-table>\r\n            <b-pagination\r\n              v-model=\"pagina\"\r\n              :total-rows=\"total\"\r\n              :per-page=\"itensPorPagina\"\r\n              align=\"right\"\r\n              size=\"md\"\r\n              class=\"mt-2\"\r\n            ></b-pagination>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </b-modal>\r\n  </a>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\nimport NotificacaoServico from \"../../servico/NotificacaoServico\";\r\n\r\nexport default {\r\n  components: { RotateSquare },\r\n\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      abrir: false,\r\n      pagina: 1,\r\n      itensPorPagina: 5,\r\n      total: 0,\r\n      itens: [],\r\n      fields: [\r\n        { key: \"data\", label: \"Data\", sortable: true },\r\n        { key: \"mensagem\", label: \"Mensagem\", sortable: true },\r\n        { key: \"lido\", label: \"Lido\", sortable: true },\r\n        {\r\n          key: \"acoes\",\r\n          label: \"Ações\",\r\n          sortable: false,\r\n          thClass: \"center, wd-120-px\"\r\n        }\r\n      ],\r\n      viewModel: null\r\n    };\r\n  },\r\n  mounted() {},\r\n  watch: {\r\n    pagina: function (val) {\r\n      this.ObterGrid(val);\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    Visualizar(item) {\r\n      this.viewModel = item;\r\n    },\r\n    AbrirModal() {\r\n      this.abrir = true;\r\n      this.viewModel = null;\r\n      this.ObterGrid(this.pagina);\r\n    },\r\n    Lido() {\r\n      this.loading = false;\r\n      NotificacaoServico.Lido(this.viewModel.id)\r\n        .then((response) => {\r\n          this.viewModel = null;\r\n          this.loading = false;\r\n          this.ObterGrid(this.pagina);\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    ObterGrid(pagina) {\r\n      this.loading = false;\r\n      NotificacaoServico.ObterGrid(pagina, this.itensPorPagina)\r\n        .then((response) => {\r\n          this.loading = false;\r\n          this.itens = response.data.itens;\r\n          this.total = response.data.total;\r\n          this.pagina = response.data.pagina;\r\n          this.itensPorPagina = response.data.itensPorPagina;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Notificacao"}]}