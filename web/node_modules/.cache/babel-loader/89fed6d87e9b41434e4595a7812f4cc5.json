dFornecedorProduto(\r\n    pagina,\r\n    itensPorPagina,\r\n    produtoId,\r\n    fornecedorVinculado,\r\n    nome\r\n  ) {\r\n    return Vue.prototype.$http({\r\n      url:\r\n        \"/fornecedorProduto/obter-grid-fornecedor-produto?pagina=\" +\r\n        pagina +\r\n        \"&ItensPorPagina=\" +\r\n        itensPorPagina +\r\n        \"&nome=\" +\r\n        nome +\r\n        \"&fornecedorVinculado=\" +\r\n        fornecedorVinculado +\r\n        \"&produtoId=\" +\r\n        produtoId,\r\n      method: \"GET\"\r\n    });\r\n  }\r\n\r\n  Remover(id) {\r\n    return Vue.prototype.$http({\r\n      url: \"fornecedorProduto/remover/\" + id,\r\n      method: \"DELETE\"\r\n    });\r\n  }\r\n\r\n  Novo(viewModel) {\r\n    return Vue.prototype.$http({\r\n      url: \"fornecedorProduto/novo\",\r\n      data: viewModel,\r\n      method: \"POST\"\r\n    });\r\n  }\r\n\r\n  EditarFornecedorProduto(\r\n    fornecedorProdutoId,\r\n    valorProduto,\r\n    quantidadeProduto,\r\n    fornecedorId\r\n  ) {\r\n    return Vue.prototype.$http({\r\n      url:\r\n        \"/fornecedorProduto/editar-produto?id=\" +\r\n        fornecedorProdutoId +\r\n        \"&valor=\" +\r\n        valorProduto +\r\n        \"&quantidade=\" +\r\n        quantidadeProduto +\r\n        \"&fornecedorId=\" +\r\n        fornecedorId,\r\n      method: \"GET\"\r\n    });\r\n  }\r\n\r\n  Adicionar(fornecedorId, produtoId) {\r\n    return Vue.prototype.$http({\r\n      url:\r\n        \"/fornecedorProduto/adicionar?fornecedorId=\" +\r\n        fornecedorId +\r\n        \"&produtoId=\" +\r\n        produtoId,\r\n      method: \"GET\"\r\n    });\r\n  }\r\n\r\n  Editar(viewModel) {\r\n    return Vue.prototype.$http({\r\n      url: \"fornecedorProduto/editar\",\r\n      data: viewModel,\r\n      method: \"PUT\"\r\n    });\r\n  }\r\n}\r\n\r\nexport default new FornecedorProdutoServico();\r\n"]}]}