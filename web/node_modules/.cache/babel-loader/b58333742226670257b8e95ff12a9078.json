           <header class=\"card-header\">\r\n              <strong class=\"align-self-center\"\r\n                >{{\r\n                  viewModel.id == this.$store.getters.emptyGuid\r\n                    ? \"Novo Tipo de Cliente\"\r\n                    : \"Editar Tipo de Cliente\"\r\n                }}\r\n              </strong>\r\n            </header>\r\n            <div class=\"card-body\">\r\n              <div class=\"row\">\r\n                <div class=\"col\">\r\n                  <div class=\"form-group\">\r\n                    <small>Campos com * são de preenchimento obrigatório</small>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"col-sm-12 col-md-4 col-lg-4 col-xl-4\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Descrição</label>\r\n                    <input\r\n                      v-model=\"viewModel.descricao\"\r\n                      class=\"form-control\"\r\n                      type=\"text\"\r\n                      placeholder=\"Digite a descricao\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"btn-toolbar mb-3 ml-3\" role=\"toolbar\">\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button class=\"btn btn-success mr-2\" type=\"submit\">\r\n                  Salvar\r\n                </button>\r\n              </div>\r\n              <div class=\"btn-group\" role=\"group\">\r\n                <button\r\n                  class=\"btn btn-secondary\"\r\n                  type=\"reset\"\r\n                  @click=\"$router.go(-1)\"\r\n                >\r\n                  Voltar\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\n\r\nexport default {\r\n  name: \"NovoTipoCliente\",\r\n  components: {\r\n    RotateSquare\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      contaOptions: [],\r\n      viewModel: {\r\n        id: this.$store.getters.emptyGuid,\r\n        descricao: \"\"\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    let tipoClienteId = this.$route.params.id;\r\n    if (tipoClienteId) this.Obter(tipoClienteId);\r\n  },\r\n  methods: {\r\n    ValidarForm(evt) {\r\n      evt.preventDefault();\r\n      if (this.viewModel.id !== this.$store.getters.emptyGuid) this.Editar();\r\n      else this.Novo();\r\n    },\r\n    Obter(tipoClienteId) {\r\n      this.loading = false;\r\n      this.$http({\r\n        url: \"tipoCliente/obter/\" + tipoClienteId,\r\n        method: \"GET\"\r\n      })\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.viewModel = resposta.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    Novo() {\r\n      this.loading = false;\r\n      this.$http({\r\n        url: \"tipoCliente/novo\",\r\n        data: this.viewModel,\r\n        method: \"POST\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/tipo-cliente\");\r\n          this.$notify({\r\n            data: [\"Tipo de cliente cadastrada com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    },\r\n    Editar() {\r\n      this.loading = false;\r\n      this.$http({\r\n        url: \"tipoCliente/editar\",\r\n        data: this.viewModel,\r\n        method: \"PUT\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/tipo-cliente\");\r\n          this.$notify({\r\n            data: [\"Tipo de cliente editada com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 5000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/TipoCliente"}]}