>\r\n    <form v-else @submit=\"ValidarForm\">\r\n      <div class=\"row\">\r\n        <div class=\"col-sm-12 col-md-12 col-lg-12 col-xl-12\">\r\n          <div class=\"card\">\r\n            <header class=\"card-header\">\r\n              <strong class=\"align-self-center\">Novo Tipo de produto</strong>\r\n            </header>\r\n            <div class=\"card-body\">\r\n              <div class=\"row\">\r\n                <div class=\"col\">\r\n                  <div class=\"form-group\">\r\n                    <small>Campos com * são de preenchimento obrigatório</small>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"col-sm-12 col-md-4 col-lg-4 col-xl-4\">\r\n                  <div class=\"form-group\">\r\n                    <label for>* Descricao</label>\r\n                    <input\r\n                      v-model=\"viewModel.descricao\"\r\n                      class=\"form-control\"\r\n                      type=\"text\"\r\n                      placeholder=\"Digite a descrição\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <button class=\"btn btn-success mr-2\" type=\"submit\">Salvar</button>\r\n          <button\r\n            class=\"btn btn-secondary\"\r\n            type=\"reset\"\r\n            @click=\"$router.push('/tipoProduto')\"\r\n          >\r\n            Voltar\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RotateSquare from \"../../components/RotateSquare\";\r\n\r\nexport default {\r\n  name: \"NovoTipoProduto\",\r\n  components: {\r\n    RotateSquare\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      contaOptions: [],\r\n      viewModel: {\r\n        id: this.$store.state.emptyGuid,\r\n        descricao: \"\"\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    let tipoProdutoId = this.$route.params.id;\r\n    if (tipoProdutoId) this.Obter(tipoProdutoId);\r\n  },\r\n  methods: {\r\n    ValidarForm(evt) {\r\n      evt.preventDefault();\r\n      if (this.viewModel.id !== this.$store.state.emptyGuid) this.Editar();\r\n      else this.Novo();\r\n    },\r\n    Obter(tipoProdutoId) {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"tipoProduto/obter/\" + tipoProdutoId,\r\n        method: \"GET\"\r\n      })\r\n        .then((resposta) => {\r\n          this.loading = false;\r\n          this.viewModel = resposta.data;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Novo() {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"tipoProduto/novo\",\r\n        data: this.viewModel,\r\n        method: \"POST\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/tipoProduto\");\r\n          this.$notify({\r\n            data: [\"Tipo de produto cadastrado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    },\r\n    Editar() {\r\n      this.loading = true;\r\n      this.$http({\r\n        url: \"tipoProduto/editar\",\r\n        data: this.viewModel,\r\n        method: \"PUT\"\r\n      })\r\n        .then(() => {\r\n          this.loading = false;\r\n          this.$router.push(\"/tipoProduto\");\r\n          this.$notify({\r\n            data: [\"Tipo de produto editado com sucesso.\"],\r\n            type: \"success\",\r\n            duration: 10000\r\n          });\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 10000\r\n          });\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}