       responsive\r\n        :items=\"itens\"\r\n        :fields=\"fields\"\r\n        striped\r\n        :per-page=\"itensPorPagina\"\r\n        show-empty\r\n        empty-text=\"Nenhum arquivo encontrado.\"\r\n      >\r\n        <template v-slot:empty=\"scope\">\r\n          <h4>{{ scope.emptyText }}</h4>\r\n        </template>\r\n        <template v-slot:cell(acoes)=\"data\">\r\n          <div class=\"btn-group-sm\">\r\n            <a\r\n              class=\"btn btn-secondary\"\r\n              :href=\"$store.getters.baseURL + 'arquivo/obter/' + data.item.arquivoId\"\r\n              target=\"_blank\"\r\n              title=\"Download arquivo\"\r\n              ><i class=\"fas fa-download\"></i\r\n            ></a>\r\n          </div>\r\n        </template>\r\n      </b-table>\r\n      <b-pagination\r\n        v-model=\"pagina\"\r\n        :total-rows=\"total\"\r\n        :per-page=\"itensPorPagina\"\r\n        align=\"right\"\r\n        size=\"md\"\r\n        class=\"mt-2\"\r\n      ></b-pagination>\r\n    </b-modal>\r\n  </b-button>\r\n</template>\r\n\r\n<script>\r\nimport DocumentoAnexoService from \"../servico/DocumentoAnexoService\";\r\nexport default {\r\n  components: {},\r\n  props: {\r\n    referenciaId: {\r\n      type: String,\r\n      default: \"\"\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      abrir: false,\r\n      loading: false,\r\n      itens: [],\r\n      pagina: 1,\r\n      total: 0,\r\n      itensPorPagina: 20,\r\n      fields: [\r\n        { key: \"nome\", label: \"Nome\", sortable: true },\r\n        {\r\n          key: \"acoes\",\r\n          label: \"Ações\",\r\n          sortable: false,\r\n          thClass: \"center, wd-120-px\"\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  watch: {\r\n    pagina: function (val) {\r\n      this.ObterGrid(val);\r\n    }\r\n  },\r\n  methods: {\r\n    AbrirModal() {\r\n      this.abrir = true;\r\n      this.pagina = 1;\r\n      this.total = 0;\r\n      this.itens = [];\r\n      this.ObterGrid(this.pagina);\r\n    },\r\n    ObterGrid(pagina) {\r\n      this.loading = false;\r\n      DocumentoAnexoService.ObterGrid(\r\n        pagina,\r\n        this.itensPorPagina,\r\n        this.referenciaId\r\n      )\r\n        .then((response) => {\r\n          this.loading = false;\r\n          this.itens = response.data.itens;\r\n          this.total = response.data.total;\r\n          this.pagina = response.data.pagina;\r\n          this.itensPorPagina = response.data.itensPorPagina;\r\n        })\r\n        .catch((erro) => {\r\n          this.loading = false;\r\n          this.$notify({\r\n            data: erro.response.data.erros,\r\n            type: \"warn\",\r\n            duration: 5000\r\n          });\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}